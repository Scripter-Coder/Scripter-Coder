-- Loop through each child in the folder
for _, glove in ipairs(unlockedGlovesFolder:GetChildren()) do
    if glove:IsA("BoolValue") then  -- Ensure the child is a BoolValue
        if glove.Value then
            print("✅" .. glove.Name)  -- Optionally, you can display the name of the glove
        else
            print("❌" .. glove.Name)
        end
    end
end
