AntiRun = AntiTab:AddToggle({
	Name = "Anti Run",
	Default = false,
	Callback = function(Value)
		_G.AutoExit = Value
		coroutine.wrap(function()
			while _G.AutoExit do
				local player = game.Players.LocalPlayer
				local character = player.Character
				if character and character:FindFirstChild("InLabyrinth") then
					for _, v in ipairs(workspace:GetChildren()) do
						if v:IsA("Model") and string.find(v.Name, "Labyrinth") and v:FindFirstChild("Doors") then
							for _, door in ipairs(v.Doors:GetChildren()) do
								if door:FindFirstChild("Hitbox") and door.Hitbox:FindFirstChild("TouchInterest") then
									local hrp = character:FindFirstChild("HumanoidRootPart")
									if hrp then
										firetouchinterest(hrp, door.Hitbox, 0)
										firetouchinterest(hrp, door.Hitbox, 1)
									end
								end
							end
						end
					end
				end
				task.wait()
			end
		end)()
	end
})

