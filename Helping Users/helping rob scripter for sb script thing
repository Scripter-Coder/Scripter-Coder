-- AntiVoid Part
function AntiVoid()
    local part = Instance.new("AntiVoid")
    part.Name = "AntiVoid"
    part.Size = Vector3.new(1000000, 3, 1000000)
    part.Position = Vector3.new(-4.08799744, -11.1922169, 1.83599997)
    part.Anchored = true
    part.Material = Enum.Material.ForceField
    part.Parent = workspace
end

function DestroyAntiVoid()
    local av = workspace:FindFirstChild("AntiVoid")
    if av then av:Destroy() end
end

-- Slap Aura with Avoid Friends
function SlapAura()
    task.spawn(function()
        while SlapAuraEnable do
            local leaderstats = LocalPlayer:FindFirstChild("leaderstats")
            local glove = leaderstats and leaderstats:FindFirstChild("Glove")
            local gloveName = glove and glove.Value
            local remoteName = gloveToRemote[gloveName]
                    local remote = remoteName and ReplicatedStorage:FindFirstChild(remoteName)
            if remote then
                for _, player in pairs(Players:GetPlayers()) do
                    if not player or not player.Name then continue end
                            if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("Right Arm") and player.Character:FindFirstChild("entered") then
                        if not player.Character:FindFirstChild("Reversed") or player.Character:FindFirstChild("UnoReverseCard") or player.Character:FindFirstChild("Counter") or player.Character:FindFirstChild("rock") then
                            if not AvoidFriends or not LocalPlayer:IsFriendsWith(player.UserId) then
                                remote:FireServer(player.Character["Right Arm"])
                            end
                        end
                    end
                end
            end
            task.wait(0.5)
        end
    end)
end

