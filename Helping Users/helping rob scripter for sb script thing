AntiRecord = AntiTab:AddToggle({
	Name = "Anti Record",
	Default = false,
	Callback = function(Value)
		_G.AntiRecord = Value
	end    
})

-- AntiRecord chat monitoring
for i,p in pairs(game.Players:GetChildren()) do
	if p ~= game.Players.LocalPlayer then
		p.Chatted:Connect(function(message)
			local Words = message:split(" ")
			if _G.AntiRecord then
				for _, v in pairs(Words) do
					local lv = v:lower()
					if lv:match("recording") or lv:match("rec") or lv:match("record") or lv:match("discor") or lv:match(" disco") or lv:match(" disc") or lv:match("ticket") or lv:match("tickets") or lv:match(" ds") or lv:match(" dc") or lv:match("dizzy") or lv:match("dizzycord") or lv:match(" clip") or lv:match("proof") or lv:match("evidence") then
						AntiKick:Set(false)
						game.Players.LocalPlayer:Kick("Possible player recording detected. [ "..p.Name.." ] [ "..message.." ]")
					end
				end
			end
		end)
	end
end

game.Players.PlayerAdded:Connect(function(Player)
	Player.Chatted:Connect(function(message)
		local Words = message:split(" ")
		if _G.AntiRecord then
			for _, v in pairs(Words) do
				local lv = v:lower()
				if lv:match("recording") or lv:match(" rec") or lv:match("record") or lv:match("discor") or lv:match(" disco") or lv:match(" disc") or lv:match("ticket") or lv:match("tickets") or lv:match(" ds") or lv:match(" dc") or lv:match("dizzy") or lv:match("dizzycord") or lv:match(" clip") or lv:match("proof") or lv:match("evidence") then
					AntiKick:Set(false)
					game.Players.LocalPlayer:Kick("Possible player recording detected. [ "..Player.Name.." ] [ "..message.." ]")
				end
			end
		end
	end)
end)

AntiRagdoll = AntiTab:AddToggle({
	Name = "Anti Ragdoll",
	Default = false,
	Callback = function(Value)
		_G.AntiRagdoll = Value
		while _G.AntiRagdoll do
			if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and game.Players.LocalPlayer.Character:FindFirstChild("Torso") and game.Players.LocalPlayer.Character:FindFirstChild("Ragdolled") then
				if game.Players.LocalPlayer.Character:FindFirstChild("Ragdolled") and game.Players.LocalPlayer.Character:WaitForChild("Ragdolled").Value == true then
					repeat task.wait()
						if game.Players.LocalPlayer.Character:FindFirstChild("Torso") then
							game.Players.LocalPlayer.Character.Torso.Anchored = true
							game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
						end
					until game.Players.LocalPlayer.Character.Ragdolled.Value == false or _G.AntiRagdoll == false
					game.Players.LocalPlayer.Character.Torso.Anchored = false
					game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
				end
			end
			task.wait()
		end
	end    
})

--Farm Tab
FarmTab:AddToggle({
	Name = "Autofarm Slapples",
	Default = false,
	Callback = function(Value)
	    SlappleFarm = Value
while SlappleFarm do
if game.Players.LocalPlayer.Character:FindFirstChild("entered") then
for i, v in pairs(workspace.Arena.island5.Slapples:GetChildren()) do
                if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and game.Players.LocalPlayer.Character:FindFirstChild("entered") and v.Name == "Slapple" or v.Name == "GoldenSlapple" and v:FindFirstChild("Glove") and v.Glove:FindFirstChildWhichIsA("TouchTransmitter") then
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Glove, 0)
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Glove, 1)
                end
            end
       end
task.wait()
end
	end    
})

FarmTab:AddToggle({
	Name = "Autofarm Candy",
	Default = false,
	Callback = function(Value)
	    CandyCornsFarm = Value
while CandyCornsFarm do
for i, v in pairs(game.Workspace.CandyCorns:GetChildren()) do
                if game.Players.LocalPlayer.Character:FindFirstChild("Head") and v:FindFirstChildWhichIsA("TouchTransmitter") then
                    firetouchinterest(game.Players.LocalPlayer.Character.Head, v, 0)
                    firetouchinterest(game.Players.LocalPlayer.Character.Head, v, 1)
                end
            end
task.wait()
end
	end    
})

-- Settings Tab Toggle & Button
local Players = game.Players
local LocalPlayer = Players.LocalPlayer
local Walkspeed = 20
local Jumppower = 50
local KeepWalkspeed = false
local KeepJumppower = false


local function setWalkspeed(value)
	if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
		LocalPlayer.Character.Humanoid.WalkSpeed = value
		Walkspeed = value
	end
end

local function setJumppower(value)
	if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
		LocalPlayer.Character.Humanoid.JumpPower = value
		Jumppower = value
	end
end

SettingsTab:AddSlider({
	Name = "WalkSpeed",
	Min = 20,
	Max = 1000,
	Default = 20,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "WalkSpeed",
	Callback = setWalkspeed
})

SettingsTab:AddTextbox({
	Name = "WalkSpeed",
	Default = "UserSpeed",
	TextDisappear = false,
	Callback = function(Value)
		local num = tonumber(Value)
		if num then setWalkspeed(num) end
	end
})

SettingsTab:AddToggle({
	Name = "Walkspeed Set Auto",
	Default = false,
	Callback = function(Value)
		KeepWalkspeed = Value
		task.spawn(function()
			while KeepWalkspeed do
				setWalkspeed(Walkspeed)
				task.wait()
			end
		end)
	end
})

SettingsTab:AddSlider({
	Name = "JumpPower",
	Min = 50,
	Max = 1000,
	Default = 50,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "JumpPower",
	Callback = setJumppower
})

SettingsTab:AddTextbox({
	Name = "Jumppower",
	Default = "UserPower",
	TextDisappear = false,
	Callback = function(Value)
		local num = tonumber(Value)
		if num then setJumppower(num) end
	end
})

SettingsTab:AddToggle({
	Name = "Jumppower Set Auto",
	Default = false,
	Callback = function(Value)
		KeepJumppower = Value
		task.spawn(function()
			while KeepJumppower do
				setJumppower(Jumppower)
				task.wait()
			end
		end)
	end
})

SettingsTab:AddButton({
	Name = "[ Destroy GUI ] [ Toggle Gui ]",
	Callback = function()
OrionLib:Destroy()
if game.Players.LocalPlayer.PlayerGui:FindFirstChild("ToggleUi") then
game.Players.LocalPlayer.PlayerGui:FindFirstChild("ToggleUi"):Destroy()
end
  	end 
})

