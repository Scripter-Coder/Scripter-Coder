--This is advanced script
--If u are skid atleast credit me üôè
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/jensonhirst/Orion/main/source')))()

if game.PlaceId == 6403373529 or game.PlaceId == 9015014224 or game.PlaceId == 11520107397 or game.PlaceId == 124596094333302 then -- Slap Battles
    local Window = OrionLib:MakeWindow({Name = "Auto Get Eggler Glove", HidePremium = false, IntroEnabled = true, IntroText = "Slap Battles"})

    local Tab1 = Window:MakeTab({
        Name = "Main",
        Icon = "rbxassetid://7733960981",
        PremiumOnly = false
    })

    local Tab2 = Window:MakeTab({
        Name = "Credits",
        Icon = "rbxassetid://7743876054",
        PremiumOnly = false
    })

    local Tab3 = Window:MakeTab({
        Name = "Settings",
        Icon = "rbxassetid://7734053495",
        PremiumOnly = false
    })

    local Section = Tab1:AddSection({
        Name = "Teleporter"
    })

    local Teleport = ""

    -- Dropdown for Teleport Location
    Tab1:AddDropdown({
        Name = "Where Teleport?",
        Default = "",
        Options = {"Guide Basement", "Arena", "Cannon", "Slapple", "1 Island", "2 Island", "3 Island"},
        Callback = function(Value)
            Teleport = Value
        end    
    })

    -- Teleport Button
    Tab1:AddButton({
        Name = "Teleport",
        Callback = function()
            local locations = {
                ["Guide Basement"] = CFrame.new(17894, -130, -3524),
                ["Arena"] = CFrame.new(0, 34, 0),  -- Update with correct coordinates
                ["Cannon"] = CFrame.new(258, 34, 198),
                ["Slapple"] = CFrame.new(-391, 51, -13),
                ["1 Island"] = CFrame.new(-14, -5, 213),
                ["2 Island"] = CFrame.new(-259, -5, 3),
                ["3 Island"] = CFrame.new(10, -5, -212)
            }
            
            if locations[Teleport] then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = locations[Teleport]
            else
                OrionLib:MakeNotification({
                    Name = "Error",
                    Content = "Please Choose a Teleport Location Before Clicking",
                    Image = "rbxassetid://7743878857",
                    Time = 3
                })
            end
        end
    })

    local Section = Tab1:AddSection({
        Name = "Egg Clicker"
    })

    local Egg = ""

    -- Dropdown for Egg Selection
    Tab1:AddDropdown({
        Name = "What Egg You Choose?",
        Default = "",
        Options = {"Black", "Blossom", "Blue", "Brown", "DarkGrey", "Green", "LightGrey", "Orange", "Red", "White"},
        Callback = function(Value)
            Egg = Value
        end    
    })

    -- Button to Click Egg
    Tab1:AddButton({
        Name = "Click Egg",
        Callback = function()
            local eggs = {
                ["Black"] = workspace.EasterHuntEggs.Black,
                ["Blossom"] = workspace.EasterHuntEggs.Blossom,
                ["Blue"] = workspace.EasterHuntEggs.Blue,
                ["Brown"] = workspace.EasterHuntEggs.Brown,
                ["DarkGrey"] = workspace.EasterHuntEggs.DarkGrey,
                ["Green"] = workspace.EasterHuntEggs.Green,
                ["LightGrey"] = workspace.EasterHuntEggs.LightGrey,
                ["Orange"] = workspace.EasterHuntEggs.Orange,
                ["Red"] = workspace.EasterHuntEggs.Red,
                ["White"] = workspace.EasterHuntEggs.White
            }

            if eggs[Egg] then
                fireclickdetector(eggs[Egg].ClickDetector)
            else
                OrionLib:MakeNotification({
                    Name = "Error",
                    Content = "Please Choose an Egg Before Clicking",
                    Image = "rbxassetid://7743878857",
                    Time = 3
                })
            end
        end
    })

    -- Section for Teleport to another place
    local PlaceChooser = ""

    Tab1:AddDropdown({
        Name = "What Type You Choose?",
        Default = "",
        Options = {"1. Teleports Using Place ID", "2. Clicks On Pig"},
        Callback = function(Value)
            PlaceChooser = Value
        end    
    })

    -- Teleport to another place
    Tab1:AddButton({
        Name = "Teleport To Place",
        Callback = function()
            if PlaceChooser == "1. Teleports Using Place ID" then
                game:GetService("TeleportService"):Teleport(129665246576996)
            elseif PlaceChooser == "2. Clicks On Pig" then
                fireclickdetector(workspace.EggTeleport.ClickDetector)
            else
                OrionLib:MakeNotification({
                    Name = "Error",
                    Content = "Please Choose a Valid Option Before Clicking",
                    Image = "rbxassetid://7743878857",
                    Time = 3
                })
            end
        end
    })

    -- Credits
    Tab2:AddLabel("Credits To: Scripter")
    Tab2:AddLabel("Scripter: Owner Of Script")

    -- Destroy GUI Button
    Tab3:AddButton({
        Name = "Destroy Gui",
        Callback = function()
            OrionLib:Destroy()
        end    
    })

end

if game.PlaceId == 129665246576996 then -- Eggler Place
    local Window = OrionLib:MakeWindow({Name = "Auto Get Eggler Glove", HidePremium = false, IntroEnabled = true, IntroText = "Eggler Place"})

    local Tab1 = Window:MakeTab({
        Name = "Main",
        Icon = "rbxassetid://7733960981",
        PremiumOnly = false
    })
    
    local Tab2 = Window:MakeTab({
        Name = "Credits",
        Icon = "rbxassetid://7743876054",
        PremiumOnly = false
    })
    
    local Tab3 = Window:MakeTab({
        Name = "Settings",
        Icon = "rbxassetid://7734053495",
        PremiumOnly = false
    })

    -- NPC Teleport
    Tab1:AddButton({
        Name = "Teleport To Kenneth",
        Callback = function()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(68, 4235, 0)
        end    
    })

    -- Trial Finisher Dropdown
    local ObbyFinisher = ""

    Tab1:AddDropdown({
        Name = "Choose What Trial To Beat?",
        Default = "",
        Options = {"1 Trial", "2 Trial", "3 Trial"},
        Callback = function(Value)
            ObbyFinisher = Value
        end    
    })

    -- Button to Beat Trial
    Tab1:AddButton({
        Name = "Beat Trial",
        Callback = function()
    if ObbyFinisher = "1 Trial" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(610, 4264, -31)
    elseif ObbyFinisher = "2 Trial" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(15, 3390, 1924)
    elseif ObbyFinisher = "3 Trial" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-6.01072836, -1766.56104, -116.879028, 0.7223894, 0, 0.691486478, 0, 1, 0, -0.691486478, 0, 0.7223894)
            else
                OrionLib:MakeNotification({
                    Name = "Error",
                    Content = "Please Select a Valid Trial",
                    Image = "rbxassetid://7743878857",
                    Time = 3
                })
            end
        end    
    })

    -- Button to Beat All Trials
    Tab1:AddButton({
        Name = "Beat All Trials",
        Callback = function()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new()
        end    
    })

    -- Credits Section
    Tab2:AddLabel("Credits To: Scripter")
    Tab2:AddLabel("Scripter: Owner Of Script")

    -- Destroy GUI Button
    Tab3:AddButton({
        Name = "Destroy Gui",
        Callback = function()
            OrionLib:Destroy()
        end    
    })
end
