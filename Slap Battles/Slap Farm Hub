-- Bot Control Variables
local botActive = false
local spamClickTool = false
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

-- Update references when character respawns
local function onCharacterAdded(char)
    character = char
    humanoid = char:WaitForChild("Humanoid")
end
player.CharacterAdded:Connect(onCharacterAdded)

-- Walk function using Pathfinding
local function walkToPosition(position)
    if not character or not humanoid or humanoid.Health <= 0 then return end
    local path = PathfindingService:CreatePath()
    path:ComputeAsync(character.HumanoidRootPart.Position, position)
    if path.Status == Enum.PathStatus.Success then
        for _, waypoint in ipairs(path:GetWaypoints()) do
            if not botActive or not character or humanoid.Health <= 0 then return end
            humanoid:MoveTo(waypoint.Position)
            local reached = humanoid.MoveToFinished:Wait()
            if not reached then break end
        end
    else
        humanoid:MoveTo(position)
        humanoid.MoveToFinished:Wait()
    end
end

-- Get random player to follow
local function getRandomPlayer()
    local players = {}
    for _, plr in pairs(Players:GetPlayers()) do
        if plr ~= player and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
            table.insert(players, plr)
        end
    end
    if #players == 0 then return nil end
    return players[math.random(#players)]
end

-- Oneshot gloves list and filter
local oneshotGloves = {
    ["Error"] = true,
    ["OVERKILL"] = true,
    ["God's Hand"] = true,
    ["The Flex"] = true
}

local ignoreOneshots = false
local ignoreMegarock = false

local function getRandomPlayerFiltered()
    local players = {}
    for _, plr in pairs(Players:GetPlayers()) do
        if plr ~= player and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
            if ignoreOneshots then
                local gloveVal = plr:FindFirstChild("leaderstats") and plr.leaderstats:FindFirstChild("Glove")
                if gloveVal and oneshotGloves[gloveVal.Value] then
                    continue
                end
            end
            table.insert(players, plr)
        end
    end
    if #players == 0 then return nil end
    return players[math.random(#players)]
end

-- Main bot loop (runs forever) - UPDATED WITH INTELLIGENCE LEVELS
task.spawn(function()
    while true do
        if botActive then
            if character and humanoid and humanoid.Health > 0 then
                if not player.Character:FindFirstChild("entered") then
                    walkToPosition(Vector3.new(-1009, 328, 4))
                else
                    local target = getRandomPlayerFiltered()
                    if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
                        local targetPos = target.Character.HumanoidRootPart.Position
                        local distance = (character.HumanoidRootPart.Position - targetPos).Magnitude
                        
                        -- Apply intelligence level behaviors
                        if intelligenceLevel == "Low" then
                            -- Simple direct movement
                            humanoid:MoveTo(targetPos)
                            
                        elseif intelligenceLevel == "Normal" then
                            -- Use pathfinding with basic logic
                            walkToPosition(targetPos)
                            
                        elseif intelligenceLevel == "High" then
                            -- Advanced behavior with strategy
                            if distance > 20 then
                                -- Far away: use pathfinding
                                walkToPosition(targetPos)
                            elseif distance > 5 then
                                -- Medium range: predict movement
                                local targetVelocity = target.Character.HumanoidRootPart.Velocity
                                local predictedPos = targetPos + (targetVelocity * 0.3) -- Predict 0.3 seconds ahead
                                humanoid:MoveTo(predictedPos)
                            else
                                -- Close range: circle strafe
                                local rightVector = (targetPos - character.HumanoidRootPart.Position):Cross(Vector3.new(0, 1, 0)).Unit
                                local strafePos = targetPos + (rightVector * 3)
                                humanoid:MoveTo(strafePos)
                            end
                        end
                        
                        -- Apply WalkSpeed and JumpPower based on intelligence
                        if intelligenceLevel == "High" then
                            humanoid.WalkSpeed = WalkSpeed + 5 -- Slight boost for high intelligence
                        else
                            humanoid.WalkSpeed = WalkSpeed
                        end
                        
                        humanoid.JumpPower = JumpPower
                        
                        -- Attack logic based on intelligence
                        if distance <= 8 then
                            local tool = nil
                            for _, item in pairs(character:GetChildren()) do
                                if item:IsA("Tool") then
                                    tool = item
                                    break
                                end
                            end
                            
                            if tool and tool.Activate then
                                if intelligenceLevel == "High" then
                                    -- High intelligence: spam click rapidly
                                    for i = 1, 3 do
                                        tool:Activate()
                                        task.wait(0.05)
                                    end
                                elseif intelligenceLevel == "Normal" then
                                    -- Normal: click occasionally
                                    tool:Activate()
                                    task.wait(0.1)
                                else
                                    -- Low: single click
                                    tool:Activate()
                                end
                            end
                        end
                        
                        -- High intelligence: occasional jumps to avoid attacks
                        if intelligenceLevel == "High" and math.random(1, 100) <= 10 then
                            humanoid.Jump = true
                        end
                        
                    else
                        task.wait(0.1)
                    end
                end
            else
                task.wait(0.5)
            end
        else
            task.wait(0.1)
        end
    end
end)

-- Spam click tool loop
task.spawn(function()
    while true do
        if spamClickTool and character and humanoid and humanoid.Health > 0 then
            local tool = nil
            for _, item in pairs(character:GetChildren()) do
                if item:IsA("Tool") then
                    tool = item
                    break
                end
            end
            if tool and tool.Activate then
                tool:Activate()
            end
        end
        task.wait(0.1)
    end
end)

