-- Dual Glove Auto Slap Bot
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")

-- Control variables
local dualBotActive = false
local dualSlappleFarm = false
local tweenSpeed = 0.5 -- Default tween speed (lower = faster)

-- Character references
local character
local humanoidRootPart

-- Update character references
local function setupCharacter()
    character = player.Character
    if character then
        humanoidRootPart = character:WaitForChild("HumanoidRootPart")
    end
end

setupCharacter()
player.CharacterAdded:Connect(function(newChar)
    character = newChar
    task.wait(0.5)
    humanoidRootPart = character:WaitForChild("HumanoidRootPart")
end)

-- Check if player is in arena (has entered)
local function isPlayerInArena(plr)
    return plr.Character and plr.Character:FindFirstChild("entered")
end

-- Check if Dual glove is equipped
local function isDualEquipped()
    local leaderstats = player:FindFirstChild("leaderstats")
    if leaderstats then
        local glove = leaderstats:FindFirstChild("Glove")
        if glove then
            return glove.Value == "Dual"
        end
    end
    return false
end

-- Get random player who has entered (excluding self)
local function getRandomEnteredPlayer()
    local players = {}
    for _, plr in pairs(Players:GetPlayers()) do
        if plr ~= player and isPlayerInArena(plr) then
            local hrp = plr.Character:FindFirstChild("HumanoidRootPart")
            if hrp then
                table.insert(players, plr)
            end
        end
    end
    if #players > 0 then
        return players[math.random(#players)]
    end
    return nil
end

-- Tween to position (FIXED VERSION)
local function tweenToPosition(position)
    if not character or not humanoidRootPart then 
        return false 
    end
    
    -- Ensure we don't try to tween to the same position (causes errors)
    if (humanoidRootPart.Position - position).Magnitude < 1 then
        return true
    end
    
    -- Create a BodyVelocity to allow flying
    local bodyVelocity = Instance.new("BodyVelocity")
    bodyVelocity.Velocity = Vector3.new(0, 0, 0)
    bodyVelocity.MaxForce = Vector3.new(40000, 40000, 40000)
    bodyVelocity.P = 10000
    bodyVelocity.Parent = humanoidRootPart
    
    -- Calculate direction and distance
    local direction = (position - humanoidRootPart.Position)
    local distance = direction.Magnitude
    local unitDirection = direction.Unit
    
    -- Calculate velocity needed to reach position in tweenSpeed seconds
    local velocity = unitDirection * (distance / tweenSpeed)
    bodyVelocity.Velocity = velocity
    
    -- Start tweening
    local startTime = tick()
    local reached = false
    
    while tick() - startTime < tweenSpeed + 2 and dualBotActive do
        if not character or not humanoidRootPart then break end
        
        -- Check if we've reached the position
        local currentDistance = (humanoidRootPart.Position - position).Magnitude
        if currentDistance < 5 then
            reached = true
            break
        end
        
        -- Update velocity (smooth approach)
        local newDirection = (position - humanoidRootPart.Position)
        local newDistance = newDirection.Magnitude
        local newUnitDirection = newDirection.Unit
        local newVelocity = newUnitDirection * (newDistance / (tweenSpeed * 0.5))
        
        bodyVelocity.Velocity = newVelocity
        
        task.wait(0.1)
    end
    
    -- Clean up
    if bodyVelocity then
        bodyVelocity:Destroy()
    end
    
    return reached
end

-- Alternative tween method using CFrames (smoother)
local function smoothFlyToPosition(position)
    if not character or not humanoidRootPart then return false end
    
    -- Create BodyVelocity for flying
    local bodyVelocity = Instance.new("BodyVelocity")
    bodyVelocity.MaxForce = Vector3.new(40000, 40000, 40000)
    bodyVelocity.P = 10000
    bodyVelocity.Parent = humanoidRootPart
    
    -- Disable gravity while flying
    local bodyGyro = Instance.new("BodyGyro")
    bodyGyro.MaxTorque = Vector3.new(40000, 40000, 40000)
    bodyGyro.P = 10000
    bodyGyro.CFrame = CFrame.new(humanoidRootPart.Position, position)
    bodyGyro.Parent = humanoidRootPart
    
    -- Calculate flying parameters
    local startPos = humanoidRootPart.Position
    local distance = (position - startPos).Magnitude
    local duration = math.max(0.3, math.min(tweenSpeed, distance / 50)) -- Adjust duration based on distance
    
    local startTime = tick()
    local reached = false
    
    while tick() - startTime < duration * 2 and dualBotActive do
        if not character or not humanoidRootPart then break end
        
        local elapsed = tick() - startTime
        local t = elapsed / duration
        
        if t >= 1 then
            t = 1
            reached = true
        end
        
        -- Smooth interpolation
        local currentPos = startPos:Lerp(position, t)
        local velocity = (position - humanoidRootPart.Position).Unit * (distance / duration)
        
        bodyVelocity.Velocity = velocity
        bodyGyro.CFrame = CFrame.new(humanoidRootPart.Position, position)
        
        if t >= 1 then break end
        task.wait(0.033) -- ~30 FPS
    end
    
    -- Clean up
    if bodyVelocity then bodyVelocity:Destroy() end
    if bodyGyro then bodyGyro:Destroy() end
    
    return reached
end

-- Use GeneralHit remote
local function useGeneralHit(target)
    if not character or not target or not target.Character then return end
    
    local targetHRP = target.Character:FindFirstChild("HumanoidRootPart")
    if not targetHRP then return end
    
    -- Get the GeneralHit remote
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    local generalHitRemote = ReplicatedStorage:FindFirstChild("GeneralHit")
    
    if generalHitRemote then
        -- Try to hit multiple times with different arguments
        for i = 1, 5 do
            pcall(function()
                -- Try different argument formats
                generalHitRemote:FireServer(targetHRP)
                generalHitRemote:FireServer(target.Character)
                generalHitRemote:FireServer(targetHRP, {
                    ["hit"] = true,
                    ["target"] = targetHRP,
                    ["player"] = target
                })
            end)
            task.wait(0.05)
        end
    else
        -- Try to find alternative remote
        local remoteNames = {"Hit", "Slap", "Attack", "Punch", "Damage"}
        for _, name in pairs(remoteNames) do
            local remote = ReplicatedStorage:FindFirstChild(name)
            if remote then
                pcall(function()
                    remote:FireServer(targetHRP)
                end)
                break
            end
        end
    end
end

-- Main Dual bot function
local function startDualBot()
    while dualBotActive and RunService.Heartbeat:Wait() do
        if character and humanoidRootPart then
            -- Check if Dual glove is still equipped
            if not isDualEquipped() then
                OrionLib:MakeNotification({
                    Name = "Error",
                    Content = "Equip Dual glove!",
                    Image = "rbxassetid://7743878857",
                    Time = 3
                })
                dualBotActive = false
                break
            end
            
            -- Get random player who has entered
            local target = getRandomEnteredPlayer()
            
            if target and target.Character then
                local targetHRP = target.Character:FindFirstChild("HumanoidRootPart")
                if targetHRP then
                    -- Fly to player position (slightly above)
                    local flyPosition = targetHRP.Position + Vector3.new(0, 5, 0)
                    
                    OrionLib:MakeNotification({
                        Name = "Flying",
                        Content = "Flying to " .. target.Name,
                        Image = "rbxassetid://4483345998",
                        Time = 1
                    })
                    
                    -- Use smooth flying
                    local reached = smoothFlyToPosition(flyPosition)
                    
                    if reached then
                        -- Use GeneralHit on player
                        useGeneralHit(target)
                        
                        OrionLib:MakeNotification({
                            Name = "Attacked",
                            Content = "Attacked " .. target.Name,
                            Image = "rbxassetid://4483345998",
                            Time = 1
                        })
                    end
                    
                    -- Wait before next target
                    task.wait(0.3)
                else
                    task.wait(0.5)
                end
            else
                -- No players found with entered, wait
                OrionLib:MakeNotification({
                    Name = "Waiting",
                    Content = "No players with entered found",
                    Image = "rbxassetid://4483345998",
                    Time = 2
                })
                task.wait(2)
            end
        else
            task.wait(1)
            setupCharacter()
        end
    end
end

-- Auto collect slapples for Dual bot
local dualSlappleThread = nil

local function startDualSlappleFarm()
    dualSlappleThread = task.spawn(function()
        while dualSlappleFarm and dualBotActive do
            if character and isDualEquipped() then
                -- Find slapples
                local island5 = workspace:FindFirstChild("Arena") and workspace.Arena:FindFirstChild("island5")
                if island5 then
                    local slapplesFolder = island5:FindFirstChild("Slapples")
                    if slapplesFolder then
                        for _, v in pairs(slapplesFolder:GetChildren()) do
                            if not dualSlappleFarm then break end
                            
                            if character:FindFirstChild("HumanoidRootPart") and 
                               (v.Name == "Slapple" or v.Name == "GoldenSlapple") then
                                local glove = v:FindFirstChild("Glove")
                                if glove and glove:FindFirstChildWhichIsA("TouchTransmitter") then
                                    -- Fly to slapple first
                                    local slapplePos = v.Position + Vector3.new(0, 2, 0)
                                    smoothFlyToPosition(slapplePos)
                                    
                                    -- Collect slapple
                                    firetouchinterest(character.HumanoidRootPart, glove, 0)
                                    task.wait()
                                    firetouchinterest(character.HumanoidRootPart, glove, 1)
                                end
                            end
                        end
                    end
                end
            end
            task.wait(0.5)
        end
    end)
end

-- Create UI Elements
local DualBotSection = Tab:AddSection({ Name = "Dual Glove Bot" })

-- Tween Speed Slider (lower = faster)
DualBotSection:AddSlider({
    Name = "Fly Speed",
    Min = 0.1,
    Max = 2,
    Default = 0.5,
    Increment = 0.1,
    ValueName = "seconds",
    Callback = function(value)
        tweenSpeed = value
        if tweenSpeed < 0.3 then
            OrionLib:MakeNotification({
                Name = "Speed Warning",
                Content = "Very fast speed may cause detection!",
                Image = "rbxassetid://7743878857",
                Time = 2
            })
        end
    end,
})

-- Main Dual Bot toggle
Tab:AddToggle({
    Name = "Auto Fly & Slap (Dual Only)",
    Default = false,
    Callback = function(value)
        if value then
            -- Check if Dual glove is equipped
            if not isDualEquipped() then
                OrionLib:MakeNotification({
                    Name = "Error",
                    Content = "Equip Dual glove first!",
                    Image = "rbxassetid://7743878857",
                    Time = 3
                })
                return
            end
            
            -- Check if we're in arena
            if not isPlayerInArena(player) then
                OrionLib:MakeNotification({
                    Name = "Warning",
                    Content = "You need to enter arena first!",
                    Image = "rbxassetid://7743878857",
                    Time = 3
                })
            end
            
            dualBotActive = true
            OrionLib:MakeNotification({
                Name = "Dual Bot Started",
                Content = "Flying to players with entered!",
                Image = "rbxassetid://4483345998",
                Time = 3
            })
            
            -- Start the bot
            task.spawn(startDualBot)
        else
            dualBotActive = false
            dualSlappleFarm = false
            OrionLib:MakeNotification({
                Name = "Dual Bot Stopped",
                Content = "Bot is now inactive",
                Image = "rbxassetid://4483345998",
                Time = 3
            })
        end
    end
})

-- Auto collect slapples toggle
Tab:AddToggle({
    Name = "Auto Collect Slapples (Dual Only)",
    Default = false,
    Callback = function(value)
        if value then
            -- Check if Dual bot is active
            if not dualBotActive then
                OrionLib:MakeNotification({
                    Name = "Error",
                    Content = "Enable Auto Fly & Slap first!",
                    Image = "rbxassetid://7743878857",
                    Time = 3
                })
                return
            end
            
            -- Check if Dual glove is equipped
            if not isDualEquipped() then
                OrionLib:MakeNotification({
                    Name = "Error",
                    Content = "Equip Dual glove first!",
                    Image = "rbxassetid://7743878857",
                    Time = 3
                })
                return
            end
            
            dualSlappleFarm = true
            startDualSlappleFarm()
            OrionLib:MakeNotification({
                Name = "Slapple Farm",
                Content = "Auto collecting slapples while flying!",
                Image = "rbxassetid://4483345998",
                Time = 3
            })
        else
            dualSlappleFarm = false
            if dualSlappleThread then
                task.cancel(dualSlappleThread)
                dualSlappleThread = nil
            end
        end
    end,
})

-- Monitor for glove changes and arena status
task.spawn(function()
    while true do
        if dualBotActive then
            if not isDualEquipped() then
                dualBotActive = false
                dualSlappleFarm = false
                OrionLib:MakeNotification({
                    Name = "Dual Bot Stopped",
                    Content = "Dual glove unequipped!",
                    Image = "rbxassetid://7743878857",
                    Time = 3
                })
            end
            
            -- Check if we're still in arena
            if not isPlayerInArena(player) then
                OrionLib:MakeNotification({
                    Name = "Warning",
                    Content = "You left the arena!",
                    Image = "rbxassetid://7743878857",
                    Time = 2
                })
            end
        end
        task.wait(1)
    end
end)

local Tab = Window:MakeTab({
    Name = "Dual",
    Icon = "rbxassetid://7733955740",
    PremiumOnly = false
})

Tab:AddToggle({
    Name = "Dual Slap Farm",
    Default = false,
    Callback = function(state)
        _G.DualSlapRunning = state

        -- Teleport to random player (not self)
        local function teleportToRandomPlayer()
            local playerList = Players:GetPlayers()
            if #playerList > 1 then
                local randomPlayer
                repeat
                    randomPlayer = playerList[math.random(1, #playerList)]
                until randomPlayer ~= LocalPlayer

                local char = randomPlayer.Character
                if char and char:FindFirstChild("HumanoidRootPart") then
                    local localHRP = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                    if localHRP then
                        localHRP.CFrame = char.HumanoidRootPart.CFrame
                    end
                end
            else
                warn("Not enough players to teleport.")
            end
        end

        -- Fire GeneralHit on all players
        local function fireGeneralHitForAllPlayers()
            for _, player in pairs(Players:GetPlayers()) do
                if player.Character then
                    local rightArm = player.Character:FindFirstChild("Right Arm") or player.Character:FindFirstChild("RightHand")
                    if rightArm then
                        ReplicatedStorage:WaitForChild("GeneralHit"):FireServer(rightArm)
                    end
                end
            end
        end

        -- Main loop
        local function runAutoDualSlapLoop()
            local leaderstats = LocalPlayer:FindFirstChild("leaderstats")
            local glove = leaderstats and leaderstats:FindFirstChild("Glove")

            if not (glove and glove.Value == "Dual") then
                OrionLib:MakeNotification({
                    Name = "Error",
                    Content = "Equip Dual glove.",
                    Image = "rbxassetid://7743878857",
                    Time = 3
                })
                _G.DualSlapRunning = false
                return
            end

            while _G.DualSlapRunning do
                task.wait(0.5) -- cooldown between hits
                teleportToRandomPlayer()
                fireGeneralHitForAllPlayers()
            end
        end

        if state then
            coroutine.wrap(runAutoDualSlapLoop)()
        else
            _G.DualSlapRunning = false
        end
    end
})

