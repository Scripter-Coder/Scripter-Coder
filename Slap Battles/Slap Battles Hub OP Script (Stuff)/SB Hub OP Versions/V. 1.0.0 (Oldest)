local Tab = Window:MakeTab({
	Name = "Settings",
	Icon = "rbxassetid://7734053495",
	PremiumOnly = false
	})

local Section = Tab:AddSection({
	Name = "Value Changer"
})

Tab:AddTextbox({
    Name = "How Many Slaps? (Visual)",  -- Label for the textbox
    Default = "",  -- Default text in the textbox
    TextDisappear = true,  -- Text disappears when focused
    Callback = function(Value)
        -- Check if the input is a valid number
        local slapNumber = tonumber(Value)
        
        if slapNumber and slapNumber > 0 then
            -- If the input is a valid number and greater than 0, update the variable
            SlapChanger = slapNumber
        else
            -- If the input is not a valid number or is less than 1, show an error message
            OrionLib:MakeNotification({
                Name = "Tip",
                Content = "Type a correct number (greater than 0)!",
                Image = "rbxassetid://7733975185", 
                Time = 3
            })
        end
    end
})

Tab:AddButton({
    Name = "Set Slaps (Visual)",  -- Label for the button
    Callback = function()   
        -- Check if SlapChanger is defined (i.e., a valid number)
        if SlapChanger then
            -- If SlapChanger is set, apply the value to the leaderstats.Slaps value
            game.Players.LocalPlayer.leaderstats.Slaps.Value = SlapChanger
        else
            -- If SlapChanger is not set, show an error message
            OrionLib:MakeNotification({
                Name = "Error",
                Content = "Please enter a valid number of slaps!",
                Image = "rbxassetid://7733975185",
                Time = 3
            })
        end
    end
})

local Section = Tab:AddSection({
	Name = "Badge Giver"
})

Tab:AddButton({
    Name = "Give All Badges (Visual)",
    Callback = function()   
loadstring(game:HttpGet('https://raw.githubusercontent.com/Pro666Pro/getallbadges_fake/main/main.lua'))()
    end
})

Tab:AddTextbox({
	Name = "Change Nametag",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)   
game.Players.PlayerAdded:Connect(function(player)
    player.CharacterAdded:Connect(function(character)
        local head = character:WaitForChild("Head")
        local nametag = head:WaitForChild("Nametag", 10)  -- Wait up to 10 seconds for Nametag
        if nametag then
            local textLabel = nametag:WaitForChild("TextLabel", 10)  -- Wait up to 10 seconds for TextLabel
            if textLabel then
                -- Set the nametag text to "Value"
                textLabel.Text = Value
            else
                warn("TextLabel not found in Nametag for player: " .. player.Name)
            end
        else
            warn("Nametag not found on player's head: " .. player.Name)
        end
    end)
end)

-- Handle players who are already in the game when the script runs
for _, player in ipairs(game.Players:GetPlayers()) do
    if player.Character then
        local head = player.Character:FindFirstChild("Head")
        if head then
            local nametag = head:FindFirstChild("Nametag")
            if nametag then
                local textLabel = nametag:FindFirstChild("TextLabel")
                if textLabel then
                    textLabel.Text = Value
                end
            end
        end
    end
end
end
})

