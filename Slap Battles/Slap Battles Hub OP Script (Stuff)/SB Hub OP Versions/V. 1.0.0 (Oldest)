local Section = Tab:AddSection({
	Name = "4 Quest"
})

local running = false -- persistent state variable outside the toggle

Tab:AddToggle({
    Name = "Ambush Enemies 500 Times",
    Default = false,
    Callback = function(AutoSlapSpace)
        running = AutoSlapSpace -- update the running state

        if not running then
            return -- stop immediately if toggled off
        end

        -- Run everything in a separate thread
        task.spawn(function()
            local Players = game:GetService("Players")
            local ReplicatedStorage = game:GetService("ReplicatedStorage")
            local LocalPlayer = Players.LocalPlayer

            -- Function to teleport to a random player
            local function teleportToRandomPlayer()
                local playerList = Players:GetPlayers()
                if #playerList > 1 then
                    local randomPlayer = nil
                    repeat
                        randomPlayer = playerList[math.random(1, #playerList)]
                    until randomPlayer ~= LocalPlayer

                    local targetChar = randomPlayer.Character
                    if targetChar and targetChar:FindFirstChild("HumanoidRootPart") then
                        LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(targetChar.HumanoidRootPart.Position + Vector3.new(0, 3, 0))
                    end
                end
            end

            -- Function to fire SpaceHit for all players
            local function fireSpaceHitForAllPlayers()
                for _, player in pairs(Players:GetPlayers()) do
                    if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                        ReplicatedStorage:WaitForChild("HtSpace"):FireServer(player.Character.HumanoidRootPart)
                    end
                end
            end

            -- Main loop
            while true do
                if LocalPlayer.leaderstats.Glove.Value == "Space" then
                    ReplicatedStorage:WaitForChild("ZeroGSound"):FireServer()
                    teleportToRandomPlayer()
                    fireSpaceHitForAllPlayers()
                    task.wait()
                else
OrionLib:MakeNotification({Name = "Error",Content = "Equip Space Glove",Image = "rbxassetid://7743878857",Time = 3})
                    break
                end
            end
        end)
    end
})

--KILLSTREAK
local Section = Tab:AddSection({
	Name = "Killstreak Mastery"
})

local Section = Tab:AddSection({
	Name = "1/2/3 Quest"
})

Tab:AddToggle({
    Name = "Autofarm Kills",
    Default = false,
    Callback = function(AutoSlapKillstreak)
        -- Create a variable to track the running state
        local running = AutoSlapKillstreak

        -- Function to teleport to a random player
        local function teleportToRandomPlayer()
            local Players = game:GetService("Players")
            local playerList = Players:GetPlayers()

            if #playerList > 1 then
                local randomPlayer = nil
                repeat
                    randomPlayer = playerList[math.random(1, #playerList)]
                until randomPlayer ~= Players.LocalPlayer

                local randomPlayerCharacter = randomPlayer.Character
                if randomPlayerCharacter and randomPlayerCharacter:FindFirstChild("HumanoidRootPart") then
                    local teleportPosition = randomPlayerCharacter.HumanoidRootPart.Position
                    Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(teleportPosition)
                end
            else
                warn("There are not enough players in the game to teleport to.")
            end
        end

        -- Function to fire KSHit for all players
        local function fireKSHitForAllPlayers()
            local Players = game:GetService("Players")
            local ReplicatedStorage = game:GetService("ReplicatedStorage")
            
            for _, player in pairs(Players:GetPlayers()) do
                if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                    local args = { [1] = player.Character.HumanoidRootPart }
                    ReplicatedStorage:WaitForChild("KSHit"):FireServer(unpack(args))
                end
            end
        end

        -- Function to run the loop logic
        local function runAutoSlapKillstreakLoop()
            -- Only proceed if the glove is "Killstreak"
            if game.Players.LocalPlayer.leaderstats.Glove.Value == "Killstreak" then
                -- Start a while loop that checks if the toggle is still true
                while running do
                    wait(0.2)  -- Delay before performing actions

                    -- Teleport to a random player
                    teleportToRandomPlayer()

                    -- Fire KSHit for all players
                    fireKSHitForAllPlayers()

                    wait(1)  -- Delay before repeating again
                end
            else
                -- Notification if the player's glove isn't "Killstreak"
OrionLib:MakeNotification({Name = "Error",Content = "Equip Killstreak Glove",Image = "rbxassetid://7743878857",Time = 3})
            end
        end

        -- Start or stop the loop based on the toggle value
        if AutoSlapKillstreak then
            -- Start the loop when toggle is true
            running = true
            runAutoSlapKillstreakLoop()
            else
            -- Stop the loop when toggle is false
            running = false
         end
        end
})

