            local playerList = Players:GetPlayers()

            if #playerList > 1 then
                local randomPlayer = nil
                repeat
                    randomPlayer = playerList[math.random(1, #playerList)]
                until randomPlayer ~= Players.LocalPlayer

                local randomPlayerCharacter = randomPlayer.Character
                if randomPlayerCharacter and randomPlayerCharacter:FindFirstChild("HumanoidRootPart") then
                    local teleportPosition = randomPlayerCharacter.HumanoidRootPart.Position
                    Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(teleportPosition)
                end
            else
                warn("There are not enough players in the game to teleport to.")
            end
        end

        -- Function to fire ReaperHit for all players
        local function fireReaperHitForAllPlayers()
            local Players = game:GetService("Players")
            local ReplicatedStorage = game:GetService("ReplicatedStorage")
            
            for _, player in pairs(Players:GetPlayers()) do
                if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                    local args = { [1] = player.Character.HumanoidRootPart }
                    ReplicatedStorage:WaitForChild("ReaperHit"):FireServer(unpack(args))
                end
            end
        end

        -- Function to run the loop logic
        local function runAutoSlapReaperLoop()
            -- Only proceed if the glove is "Reaper"
            if game.Players.LocalPlayer.leaderstats.Glove.Value == "Reaper" then
                -- Start a while loop that checks if the toggle is still true
                while running do
                    wait(0.2)  -- Delay before performing actions

                    -- Teleport to a random player
                    teleportToRandomPlayer()

                    -- Fire ReaperHit for all players
                    fireReaperHitForAllPlayers()

                    wait(1)  -- Delay before repeating again
                end
            else
                -- Notification if the player's glove isn't "Reaper"
OrionLib:MakeNotification({Name = "Error",Content = "Equip Reaper",Image = "rbxassetid://7743878857",Time = 3})
            end
        end

        -- Start or stop the loop based on the toggle value
        if AutoSlapReaper then
            -- Start the loop when toggle is true
            running = true
            runAutoSlapPlagueLoop()
            else
            -- Stop the loop when toggle is false
            running = false
         end
        end
})

