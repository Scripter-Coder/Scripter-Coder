	Name = "WalkSpeed",
	Default = "UserSpeed",
	TextDisappear = false,
	Callback = function(Value)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
Walkspeed = Value
	end	  
})

Tab:AddToggle({
	Name = "Walkspeed Set Auto",
	Default = false,
	Callback = function(Value)
		KeepWalkspeed = Value
            while KeepWalkspeed do
                if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") ~= nil and game.Players.LocalPlayer.Character.Humanoid.WalkSpeed ~= Walkspeed then
                    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Walkspeed
                end
task.wait()
            end
	end    
})

local Section = Tab:AddSection({
	Name = "3 Quest"
})

local AutoAbsorb = false

Tab:AddToggle({
    Name = "Auto Absorb 1000 People",
    Default = false,
    Callback = function(Value)
        AutoAbsorb = Value
        local player = game.Players.LocalPlayer

        if AutoAbsorb then
            if player.leaderstats.Glove.Value:lower() == "rob" then
                task.spawn(function()
                    while AutoAbsorb do
                        local players = game:GetService("Players"):GetPlayers()

                        -- Choose a random player (not self)
                        local target = nil
                        repeat
                            target = players[math.random(1, #players)]
                        until target ~= player and target.Character and target.Character:FindFirstChild("HumanoidRootPart")

                        -- Teleport to the target player
                        local targetHRP = target.Character:FindFirstChild("HumanoidRootPart")
                        if targetHRP and player.Character:FindFirstChild("HumanoidRootPart") then
                            player.Character.HumanoidRootPart.CFrame = CFrame.new(targetHRP.Position + Vector3.new(0, 0, 2))
                        end

                        -- Fire the absorb ability
                        local replicatedStorage = game:GetService("ReplicatedStorage")
                        replicatedStorage:FindFirstChild("Ability"):FireServer()

                        task.wait(1)
                    end
                end)
            else
                AutoAbsorb = false
                OrionLib:MakeNotification({
                    Name = "Error",
                    Content = "Equip Rob Glove",
                    Image = "rbxassetid://7743878857",
                    Time = 3
                })
            end
        end
    end
})

Tab:AddButton({
	Name = "Inf Rob Transformation",
	Callback = function()
while true do
	fireclickdetector(game:GetService("Workspace").Lobby.rob.ClickDetector)
	wait(0.5)
	game:GetService("ReplicatedStorage").rob:FireServer()
	wait(17)
end
	end    
})

