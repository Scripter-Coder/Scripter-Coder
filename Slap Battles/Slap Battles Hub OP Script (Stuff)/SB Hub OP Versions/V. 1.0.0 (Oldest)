    if player.Character and player.Character:FindFirstChild("Head") then
        local args = {
            player.Character.Head,  -- Use the player's Head
            true  -- Second argument is true as per your example
        }

        -- Fire the event for each player's Head
        replicatedStorage.GeneralHit:FireServer(unpack(args))
    end
end
wait(1)
end
end
})

