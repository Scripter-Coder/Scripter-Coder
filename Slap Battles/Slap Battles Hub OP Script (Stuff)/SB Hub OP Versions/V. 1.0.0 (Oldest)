local Section = Tab:AddSection({
	Name = "2/3 Quest"
})

Tab:AddButton({
    Name = "Auto Slap (Slap Aura)",
    Callback = function()   
-- Get the ReplicatedStorage service
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Players = game:GetService("Players")

-- Function to fire the server for each player's torso
local function fireForAllPlayers()
    -- Loop through each player in the game
    for _, player in pairs(Players:GetPlayers()) do
        -- Ensure the player has a character and it contains the "Torso"
        if player.Character and player.Character:FindFirstChild("Torso") then
            -- Create the arguments table for the FireServer function
            local args = {
                [1] = player.Character.Torso
            }

            -- Fire the server with the torso part as an argument
            ReplicatedStorage.GeneralHit:FireServer(unpack(args))
        end
    end
end

-- Start the loop to fire for all players repeatedly
while true do
    fireForAllPlayers()  -- Fire for all players
    wait(1)  -- Wait 1 second before running the loop again (you can adjust the wait time)
end
end
})

