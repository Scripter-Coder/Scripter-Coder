Tab:AddToggle({
    Name = "Auto Get Megarock Glove",
    Default = false,
    Callback = function(Value)
        if Value then
            local player = game.Players.LocalPlayer
            if player.leaderstats.Glove.Value == "Diamond" then
                player.Character.HumanoidRootPart.CFrame = CFrame.new(10001, -42, 10000)
                game:GetService("ReplicatedStorage").Rockmode:FireServer()
            else
OrionLib:MakeNotification({Name = "Error",Content = "Equip Diamond Glove/Use In Arena",Image = "rbxassetid://7743878857",Time = 3})
            end
        end
    end
})

Tab:AddToggle({
	Name = "Auto Get Mitten/Snowball Gloves",
	Default = false,
	Callback = function(Value)
Giftfarm = Value
while Giftfarm do
for i,v in pairs(game.Workspace:GetChildren()) do
                    if v.Name == "Gift" then
firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"), v, 0)
firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"), v, 1)
                    end
                end
task.wait()
end
end
})

Tab:AddToggle({
	Name = "Auto Get Phase/Jet Gloves",
	Default = false,
	Callback = function(Value)
_G.PhaseOrJetfarm = Value
while _G.PhaseOrJetfarm do
for i,v in pairs(game.Workspace:GetChildren()) do
                    if v.Name == "JetOrb" or v.Name == "PhaseOrb" then
firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("Head"), v, 0)
firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("Head"), v, 1)
                    end
                end
task.wait()
end
	end    
})

Tab:AddToggle({
    Name = "Auto Get Bubble Glove",
    Default = false,
    Callback = function(AutoSlapKillstreak)
        -- Create a variable to track the running state
        local running = AutoSlapKillstreak

        -- Function to teleport to a random player
        local function teleportToRandomPlayer()
            local Players = game:GetService("Players")
            local playerList = Players:GetPlayers()

            if #playerList > 1 then
                local randomPlayer = nil
                repeat
                    randomPlayer = playerList[math.random(1, #playerList)]
                until randomPlayer ~= Players.LocalPlayer

                local randomPlayerCharacter = randomPlayer.Character
                if randomPlayerCharacter and randomPlayerCharacter:FindFirstChild("HumanoidRootPart") then
                    local teleportPosition = randomPlayerCharacter.HumanoidRootPart.Position
                    Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(teleportPosition)
                end
            else
                warn("There are not enough players in the game to teleport to.")
            end
        end

        -- Function to fire KSHit for all players
        local function fireKSHitForAllPlayers()
            local Players = game:GetService("Players")
            local ReplicatedStorage = game:GetService("ReplicatedStorage")
            
            for _, player in pairs(Players:GetPlayers()) do
                if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                    local args = { [1] = player.Character.HumanoidRootPart }
                    ReplicatedStorage:WaitForChild("KSHit"):FireServer(unpack(args))
                end
            end
        end

        -- Function to run the loop logic
        local function runAutoSlapPlagueLoop()
            -- Only proceed if the glove is "Plague"
            if game.Players.LocalPlayer.leaderstats.Glove.Value == "Killstreak" then
                -- Start a while loop that checks if the toggle is still true
                while running do
                    wait(0.2)  -- Delay before performing actions

                    -- Teleport to a random player
                    teleportToRandomPlayer()

                    -- Fire PlagueHit for all players
                    fireKSHitForAllPlayers()

                    wait(1)  -- Delay before repeating again
                end
            else
                -- Notification if the player's glove isn't "Killstreak"
OrionLib:MakeNotification({Name = "Error",Content = "Equip Killstreak",Image = "rbxassetid://7743878857",Time = 3})
            end
        end

        -- Start or stop the loop based on the toggle value
        if AutoSlapKillstreak then
            -- Start the loop when toggle is true
            running = true
            runAutoSlapKillstreakLoop()
            else
            -- Stop the loop when toggle is false
            running = false
         end
        end
})

Tab:AddButton({
    Name = "Auto Get Bubble Glove 2 (Slap Aura)",
    Callback = function()
if game.Players.LocalPlayer.leaderstats.Glove.Value == "Killstreak" then
-- Get the ReplicatedStorage and Players service
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Players = game:GetService("Players")

-- Start the infinite loop to fire for all players
while true do
    -- Loop through each player in the game
    for _, player in pairs(Players:GetPlayers()) do
        -- Ensure the player has a character and it contains the "Right Arm"
        if player.Character and player.Character:FindFirstChild("Right Arm") then
            -- Create the arguments table for the FireServer function
            local args = {
                [1] = player.Character["Right Arm"]
            }

            -- Fire the server with the "Right Arm" part as an argument
            ReplicatedStorage.KSHit:FireServer(unpack(args))
        end
    end

    -- Wait 1 second before running the loop again (adjust the wait time as needed)
    wait(1)
end
else
OrionLib:MakeNotification({Name = "Error",Content = "Equip Killstreak",Image = "rbxassetid://7743878857",Time = 3})
end
end
})

local Potatofarm = false

Tab:AddToggle({
	Name = "Auto Get Potato Glove",
	Default = false,
	Callback = function(Value)
		Potatofarm = Value
		if Potatofarm then
			local player = game.Players.LocalPlayer
			if player.leaderstats.Glove.Value == "Reverse" then
				task.spawn(function()
					while Potatofarm do
						for _, v in pairs(game.Workspace:GetChildren()) do
							if v.Name == "BubbleObject" then
								local hrp = player.Character:FindFirstChild("HumanoidRootPart")
								if hrp then
									firetouchinterest(hrp, v, 0)
									firetouchinterest(hrp, v, 1)
								end
							end
						end
						task.wait(0.1) -- avoid tight looping
					end
				end)
			else
OrionLib:MakeNotification({Name = "Error",Content = "Equip Reverse/Use Ability",Image = "rbxassetid://7743878857",Time = 3})
			end
		end
	end
})

