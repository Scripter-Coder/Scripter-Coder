								esp.Adornee = orb.PrimaryPart
								esp.Size = UDim2.new(0, 150, 0, 40)
								esp.StudsOffset = Vector3.new(0, 2, 0)
								esp.AlwaysOnTop = true

								local label = Instance.new("TextLabel", esp)
								label.BackgroundTransparency = 1
								label.Size = UDim2.new(1, 0, 1, 0)
								label.TextSize = 16
								label.Font = Enum.Font.FredokaOne
								label.TextColor3 = Color3.fromRGB(0, 255, 255) -- Cyan
								label.TextStrokeTransparency = 0.2
								label.Text = "Siphon Orb"
							end
							local gui = orb.PrimaryPart:FindFirstChild("SiphonOrbESP")
							local label = gui and gui:FindFirstChildOfClass("TextLabel")
							if label then
								local dist = math.floor((orb.PrimaryPart.Position - hrp.Position).Magnitude)
								label.Text = "Siphon Orb | " .. dist .. "m"
							end
						end
					end
				end
				task.wait(0.5)
			end
		end)
	end    
})

Tab:AddToggle({
	Name = "Materialize Orb ESP",
	Default = false,
	Callback = function(Value)
		MaterializeOrbESP = Value

		if not Value then
			for _, v in pairs(workspace:GetDescendants()) do
				if v:IsA("BillboardGui") and v.Name == "MaterializeOrbESP" then
					v:Destroy()
				end
			end
			return
		end

		task.spawn(function()
			while MaterializeOrbESP do
				local lp = game.Players.LocalPlayer
				local char = lp.Character
				if char and char:FindFirstChild("HumanoidRootPart") then
					local hrp = char.HumanoidRootPart
					for _, orb in pairs(workspace:GetDescendants()) do
						if orb:IsA("Model") and orb.Name == "MATERIALIZEOrb" and orb:FindFirstChild("PrimaryPart") then
							if not orb.PrimaryPart:FindFirstChild("MaterializeOrbESP") then
								local esp = Instance.new("BillboardGui", orb.PrimaryPart)
								esp.Name = "MaterializeOrbESP"
								esp.Adornee = orb.PrimaryPart
								esp.Size = UDim2.new(0, 150, 0, 40)
								esp.StudsOffset = Vector3.new(0, 2, 0)
								esp.AlwaysOnTop = true

								local label = Instance.new("TextLabel", esp)
								label.BackgroundTransparency = 1
								label.Size = UDim2.new(1, 0, 1, 0)
								label.TextSize = 16
								label.Font = Enum.Font.FredokaOne
								label.TextColor3 = Color3.fromRGB(0, 170, 255) -- Blue
								label.TextStrokeTransparency = 0.2
								label.Text = "Materialize Orb"
							end
							local gui = orb.PrimaryPart:FindFirstChild("MaterializeOrbESP")
							local label = gui and gui:FindFirstChildOfClass("TextLabel")
							if label then
								local dist = math.floor((orb.PrimaryPart.Position - hrp.Position).Magnitude)
								label.Text = "Materialize Orb | " .. dist .. "m"
							end
						end
					end
				end
				task.wait(0.5)
			end
		end)
	end    
})

Tab:AddToggle({
	Name = "Toolbox ESP",
	Default = false,
	Callback = function(Value)
		ToolboxESP = Value

		-- Cleanup existing ESPs
		if not Value then
			for _, v in pairs(workspace:GetDescendants()) do
				if v:IsA("BillboardGui") and v.Name == "ToolboxESP" then
					v:Destroy()
				end
			end
			return
		end

		task.spawn(function()
			while ToolboxESP do
				local lp = game.Players.LocalPlayer
				local char = lp.Character
				if not (char and char:FindFirstChild("HumanoidRootPart")) then
					task.wait(0.5)
					continue
				end
				local hrp = char.HumanoidRootPart

				-- Find Toolbox
				local success, toolboxModel = pcall(function()
					return workspace:FindFirstChild("Toolbox")
				end)

				if success and toolboxModel and toolboxModel:IsA("Model") and toolboxModel:FindFirstChild("PrimaryPart") then
					local part = toolboxModel.PrimaryPart

					if not part:FindFirstChild("ToolboxESP") then
						local esp = Instance.new("BillboardGui", part)
						esp.Name = "ToolboxESP"
						esp.Adornee = part
						esp.Size = UDim2.new(0, 150, 0, 40)
						esp.StudsOffset = Vector3.new(0, 2, 0)
						esp.AlwaysOnTop = true

						local label = Instance.new("TextLabel", esp)
						label.BackgroundTransparency = 1
						label.Size = UDim2.new(1, 0, 1, 0)
						label.TextSize = 16
						label.Font = Enum.Font.FredokaOne
						label.TextColor3 = Color3.fromRGB(139, 69, 19) -- Brown
						label.TextStrokeTransparency = 0.2
						label.Text = "Toolbox"
					end

					-- Update distance
					local gui = part:FindFirstChild("ToolboxESP")
					local label = gui and gui:FindFirstChildOfClass("TextLabel")
					if label then
						local dist = math.floor((part.Position - hrp.Position).Magnitude)
						label.Text = "Toolbox | " .. dist .. "m"
					end
				end

				task.wait(0.5)
			end
		end)
	end
})

Tab:AddToggle({
    Name = "Duck ESP",
    Default = false,
    Callback = function(Value)
        DuckESP = Value

        -- Clear existing ESPs when toggled off
        if not Value then
            for _, v in pairs(workspace:GetDescendants()) do
                if v:IsA("BillboardGui") and v.Name == "DuckESP" then
                    v:Destroy()
                end
            end
            return
        end

        -- Main loop to create/update ESP
        task.spawn(function()
            while DuckESP do
                local lp = game.Players.LocalPlayer
                local char = lp.Character
                local hrp = char and char:FindFirstChild("HumanoidRootPart")

                -- Check if HumanoidRootPart is available
                if not hrp then
                    task.wait(0.5)
                    continue
                end

                -- Get the Rubber Ducky model at the specified path
                local duckModel = workspace.Arena["default island"]:FindFirstChild("Rubber Ducky")

                -- Check if the Rubber Ducky is valid and of class Part
                if duckModel and duckModel:IsA("Part") then
                    -- Create ESP if not already created
                    if not duckModel:FindFirstChild("DuckESP") then
                        local esp = Instance.new("BillboardGui", duckModel)
                        esp.Name = "DuckESP"
                        esp.Adornee = duckModel
                        esp.Size = UDim2.new(0, 150, 0, 40)
                        esp.StudsOffset = Vector3.new(0, 2, 0)
                        esp.AlwaysOnTop = true

                        local label = Instance.new("TextLabel", esp)
                        label.BackgroundTransparency = 1
                        label.Size = UDim2.new(1, 0, 1, 0)
                        label.TextSize = 16
                        label.Font = Enum.Font.FredokaOne
                        label.TextColor3 = Color3.fromRGB(255, 255, 102) -- Yellow
                        label.TextStrokeTransparency = 0.2
                        label.Text = "Duck"
                    end

                    -- Update the distance label
                    local gui = duckModel:FindFirstChild("DuckESP")
                    local label = gui and gui:FindFirstChildOfClass("TextLabel")
                    if label then
                        local dist = math.floor((duckModel.Position - hrp.Position).Magnitude)
                        label.Text = "Duck | " .. dist .. "m"
                    end
                end

                task.wait(0.5)
            end
        end)
    end
})

Tab:AddToggle({
	Name = "Orange ESP",
	Default = false,
	Callback = function(Value)
		OrangeESP = Value

		-- Clear existing ESPs if turned off
		if not Value then
			for _, v in pairs(workspace:GetDescendants()) do
				if v:IsA("BillboardGui") and v.Name == "OrangeESP" then
					v:Destroy()
				end
			end
			return
		end

		-- Main ESP loop
		task.spawn(function()
			while OrangeESP do
				local lp = game.Players.LocalPlayer
				local char = lp.Character
				if not (char and char:FindFirstChild("HumanoidRootPart")) then
					task.wait(0.5)
					continue
				end
				local hrp = char.HumanoidRootPart

				-- Try to find the Orange model
				local orangeModel
				local success, result = pcall(function()
					return workspace.Arena.island5:FindFirstChild("Orange")
				end)

				if success and result and result:IsA("MeshPart") then
					orangeModel = result
				end

				if orangeModel then
					local part = orangeModel

					if not part:FindFirstChild("OrangeESP") then
						local esp = Instance.new("BillboardGui", part)
						esp.Name = "OrangeESP"
						esp.Adornee = part
						esp.Size = UDim2.new(0, 150, 0, 40)
						esp.StudsOffset = Vector3.new(0, 2, 0)
						esp.AlwaysOnTop = true

						local label = Instance.new("TextLabel", esp)
						label.BackgroundTransparency = 1
						label.Size = UDim2.new(1, 0, 1, 0)
						label.TextSize = 16
						label.Font = Enum.Font.FredokaOne
						label.TextColor3 = Color3.fromRGB(255, 165, 0) -- Orange
						label.TextStrokeTransparency = 0.2
						label.Text = "Orange"
					end

					-- Update label with the current distance
					local gui = part:FindFirstChild("OrangeESP")
					local label = gui and gui:FindFirstChildOfClass("TextLabel")
					if label then
						local dist = math.floor((part.Position - hrp.Position).Magnitude)
						label.Text = "Orange | " .. dist .. "m"
					end
				end

				task.wait(0.5)
			end
		end)
	end
})

