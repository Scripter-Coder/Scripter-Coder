local Tab = Window:MakeTab({
	Name = "Slap Farm",
	Icon = "rbxassetid://7733955740",
	PremiumOnly = false
	})

local Section = Tab:AddSection({
	Name = "Dual"
})

Tab:AddToggle({
    Name = "Dual Slap Farm",
    Default = false,
    Callback = function(AutoSlapDual)
        -- Create a variable to track the running state
        local running = AutoSlapDual

        -- Function to teleport to a random player
        local function teleportToRandomPlayer()
            local Players = game:GetService("Players")
            local playerList = Players:GetPlayers()

            if #playerList > 1 then
                local randomPlayer = nil
                repeat
                    randomPlayer = playerList[math.random(1, #playerList)]
                until randomPlayer ~= Players.LocalPlayer

                local randomPlayerCharacter = randomPlayer.Character
                if randomPlayerCharacter and randomPlayerCharacter:FindFirstChild("HumanoidRootPart") then
                    local teleportPosition = randomPlayerCharacter.HumanoidRootPart.Position
                    Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(teleportPosition)
                end
            else
                warn("There are not enough players in the game to teleport to.")
            end
        end

        -- Function to fire GeneralHit for all players
        local function fireGeneralHitForAllPlayers()
            local Players = game:GetService("Players")
            local ReplicatedStorage = game:GetService("ReplicatedStorage")
            
            for _, player in pairs(Players:GetPlayers()) do
                if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                    local args = { [1] = player.Character.HumanoidRootPart }
                    ReplicatedStorage:WaitForChild("GeneralHit"):FireServer(unpack(args))
                end
            end
        end

        -- Function to run the loop logic
        local function runAutoSlapDualLoop()
            -- Only proceed if the glove is "Killstreak"
            if game.Players.LocalPlayer.leaderstats.Glove.Value == "Dual" then
                -- Start a while loop that checks if the toggle is still true
                while running do
                    wait(0.2)  -- Delay before performing actions

                    -- Teleport to a random player
                    teleportToRandomPlayer()

                    -- Fire KSHit for all players
                    fireGeneralHitForAllPlayers()

                    wait(1)  -- Delay before repeating again
                end
            else
                -- Notification if the player's glove isn't "Dual"
OrionLib:MakeNotification({Name = "Error",Content = "Equip Dual Glove",Image = "rbxassetid://7743878857",Time = 3})
            end
        end

        -- Start or stop the loop based on the toggle value
        if AutoSlapDual then
            -- Start the loop when toggle is true
            running = true
            runAutoSlapDualLoop()
            else
            -- Stop the loop when toggle is false
            running = false
         end
        end
})

local Section = Tab:AddSection({
	Name = "Engineer"
})

local EngineerFarm = false

Tab:AddToggle({
	Name = "Engineer Slap Farm (Someone Script/U Engineer)",
	Default = false,
	Callback = function(Value)
		EngineerFarm = Value
		if EngineerFarm then
			task.spawn(function()
				while EngineerFarm do
					local turret = game.workspace:FindFirstChild("Turret")
					if turret and turret:FindFirstChild("HingePart") and turret.HingePart:FindFirstChild("Glove") then
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = turret.HingePart.Glove.CFrame
					end
					task.wait()
				end
			end)
		end
	end
})

local Section = Tab:AddSection({
	Name = "Baller"
})

FarmBaller = Tab:AddToggle({
	Name = "Baller Slap Farm",
	Default = false,
	Callback = function(Value)
_G.BallerFarm = Value
if game.Players.LocalPlayer.leaderstats.Glove.Value == "Baller" and game.Players.LocalPlayer.Character:FindFirstChild("entered") then
if _G.BallerFarm == true then
coroutine.wrap(SpamBaller)()
end
while _G.BallerFarm and game.Players.LocalPlayer.leaderstats.Glove.Value == "Baller" do
for _, v in pairs(workspace:GetChildren()) do
                 if v.Name:match(game.Players.LocalPlayer.Name) and v:FindFirstChild("HumanoidRootPart") then
game.ReplicatedStorage.GeneralHit:FireServer(v:WaitForChild("HumanoidRootPart"),true)
                end
            end
task.wait()
end
elseif _G.BallerFarm == true then
OrionLib:MakeNotification({Name = "Error",Content = "Equip Baller Glove/Use In Default Arena",Image = "rbxassetid://7743878857",Time = 3})
wait(0.05)
FarmBaller:Set(false)
end
  	end    
})

local Section = Tab:AddSection({
	Name = "Slapples"
})

Tab:AddToggle({
	Name = "Auto Collect Slapples",
	Default = false,
	Callback = function(Value)
	    SlappleFarm = Value
while SlappleFarm do
if game.Players.LocalPlayer.Character:FindFirstChild("entered") then
for i, v in pairs(workspace.Arena.island5.Slapples:GetChildren()) do
                if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and game.Players.LocalPlayer.Character:FindFirstChild("entered") and v.Name == "Slapple" or v.Name == "GoldenSlapple" and v:FindFirstChild("Glove") and v.Glove:FindFirstChildWhichIsA("TouchTransmitter") then
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Glove, 0)
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Glove, 1)
                end
            end
       end
task.wait()
end
end    
})

