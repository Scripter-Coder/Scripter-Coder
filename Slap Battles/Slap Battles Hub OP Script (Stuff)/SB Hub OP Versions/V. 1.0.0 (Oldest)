local Section = Tab:AddSection({
	Name = "Bomb Mastery"
})

local Section = Tab:AddSection({
	Name = "1/2/3 Quest"
})

Tab:AddButton({
    Name = "Throw Bomb/Explode Bomb",
    Callback = function()   
if game.Players.LocalPlayer.leaderstats.Glove.Value == "Bomb" then
local args = {
    [1] = "Ebutton"
}

game:GetService("ReplicatedStorage").BombThrow:FireServer(unpack(args))
else
OrionLib:MakeNotification({Name = "Error",Content = "Equip Bomb",Image = "rbxassetid://7743878857",Time = 3})
end
end
})

Tab:AddButton({
    Name = "Auto Do 1/2/3 Tasks",
    Default = false,
    Callback = function(AutoBomb)
if game.Players.LocalPlayer.leaderstats.Glove.Value == "Bomb" then
while AutoBomb do
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-908, 328, 5)
wait(1)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(10019, -42, 9960)
wait(1)
local args = {
    [1] = "Ebutton"
}

game:GetService("ReplicatedStorage").BombThrow:FireServer(unpack(args))
wait(1)
while true do
    local foundBomb = false

    for _, v in pairs(game.Workspace:GetChildren()) do
        if string.find(v.Name, "_bømb") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
            foundBomb = true  -- Set the flag to true when _bømb part is found
            break  -- Exit the loop once the _bømb part is found
        end
    end

    if not foundBomb then
        wait(1)  -- Wait before checking again, this prevents overloading the loop
    else
        wait()  -- Keeps the loop running smoothly when a bomb is found
    end
end
wait(9)
game.Players.LocalPlayer.Character.Humanoid.Health = 0
wait(3)
wait(20)
end
else
OrionLib:MakeNotification({Name = "Error", Content = "Equip Bomb Glove", Image = "rbxassetid://7743878857", Time = 3})
        end
    end
})

