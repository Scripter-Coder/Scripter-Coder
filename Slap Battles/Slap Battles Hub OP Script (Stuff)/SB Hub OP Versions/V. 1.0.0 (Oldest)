local Tab = Window:MakeTab({
	Name = "ESP",
	Icon = "rbxassetid://7743872929",
	PremiumOnly = false
	})

local Section = Tab:AddSection({
	Name = "ESP Things"
})

Tab:AddToggle({
	Name = "Players ESP",
	Default = false,
	Callback = function(Value)
		PlayerESP = Value

        -- Remove ESP when toggled off
		if PlayerESP == false then
			for i, v in ipairs(game.Players:GetChildren()) do
				if v.Character and v.Character:FindFirstChild("Head") and v.Character.Head:FindFirstChild("PlayerEsp") then
					v.Character.Head.PlayerEsp:Destroy()
				end
			end
		end

        -- Main ESP loop
		while PlayerESP do
			for i, v in ipairs(game.Players:GetChildren()) do
				local localPlayer = game.Players.LocalPlayer
				if v ~= localPlayer and v.Character and v.Character:FindFirstChild("Head") and v.Character:FindFirstChild("Humanoid") and v.Character:FindFirstChild("HumanoidRootPart") then
					if not v.Character.Head:FindFirstChild("PlayerEsp") then
						local esp = Instance.new("BillboardGui", v.Character.Head)
						esp.Name = "PlayerEsp"
						esp.Adornee = v.Character.Head
						esp.Size = UDim2.new(0, 200, 0, 50)
						esp.StudsOffset = Vector3.new(0, 3, 0)
						esp.AlwaysOnTop = true

						local label = Instance.new("TextLabel", esp)
						label.BackgroundTransparency = 1
						label.Size = UDim2.new(1, 0, 1, 0)
						label.TextSize = 16
						label.Font = Enum.Font.FredokaOne
						label.TextColor3 = Color3.new(1, 1, 1) -- White
						label.TextStrokeTransparency = 0

						local distance = math.floor((v.Character.HumanoidRootPart.Position - localPlayer.Character.HumanoidRootPart.Position).Magnitude)
						label.Text = v.Name .. " | " .. distance .. "m | HP: " .. math.floor(v.Character.Humanoid.Health)
					else
						-- Update text info
						local gui = v.Character.Head:FindFirstChild("PlayerEsp")
						if gui and gui:FindFirstChildOfClass("TextLabel") then
							local label = gui:FindFirstChildOfClass("TextLabel")
							local distance = math.floor((v.Character.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude)
							label.Text = v.Name .. " | " .. distance .. "m | HP: " .. math.floor(v.Character.Humanoid.Health)
						end
					end
				end
			end
			task.wait(0.1)
		end
	end    
})

Tab:AddToggle({
	Name = "Jet Orb ESP",
	Default = false,
	Callback = function(Value)
		JetOrbESP = Value

		-- Clear existing ESPs if disabled
		if not JetOrbESP then
			for _, v in pairs(workspace:GetDescendants()) do
				if v:IsA("BillboardGui") and v.Name == "JetOrbESP" then
					v:Destroy()
				end
			end
			return
		end

		-- Run ESP in separate thread
		task.spawn(function()
			while JetOrbESP do
				local lp = game.Players.LocalPlayer
				local char = lp.Character
				if char and char:FindFirstChild("HumanoidRootPart") then
					local hrp = char.HumanoidRootPart

					for _, orb in pairs(workspace:GetDescendants()) do
						if orb:IsA("Model") and orb.Name == "JetOrb" and orb:FindFirstChild("PrimaryPart") then
							if not orb.PrimaryPart:FindFirstChild("JetOrbESP") then
								local esp = Instance.new("BillboardGui", orb.PrimaryPart)
								esp.Name = "JetOrbESP"
								esp.Adornee = orb.PrimaryPart
								esp.Size = UDim2.new(0, 150, 0, 40)
								esp.StudsOffset = Vector3.new(0, 2, 0)
								esp.AlwaysOnTop = true

								local label = Instance.new("TextLabel", esp)
								label.BackgroundTransparency = 1
								label.Size = UDim2.new(1, 0, 1, 0)
								label.TextSize = 16
								label.Font = Enum.Font.FredokaOne
								label.TextColor3 = Color3.fromRGB(255, 255, 0) -- Yellow
								label.TextStrokeTransparency = 0.2
								local dist = math.floor((orb.PrimaryPart.Position - hrp.Position).Magnitude)
								label.Text = "Jet Orb | " .. dist .. "m"
							else
								local gui = orb.PrimaryPart:FindFirstChild("JetOrbESP")
								local label = gui and gui:FindFirstChildOfClass("TextLabel")
								if label then
									local dist = math.floor((orb.PrimaryPart.Position - hrp.Position).Magnitude)
									label.Text = "Jet Orb | " .. dist .. "m"
								end
							end
						end
					end
				end
				task.wait(0.5)
			end
		end)
	end    
})

Tab:AddToggle({
	Name = "Phase Orb ESP",
	Default = false,
	Callback = function(Value)
		PhaseOrbESP = Value

		if not Value then
			for _, v in pairs(workspace:GetDescendants()) do
				if v:IsA("BillboardGui") and v.Name == "PhaseOrbESP" then
					v:Destroy()
				end
			end
			return
		end

		task.spawn(function()
			while PhaseOrbESP do
				local localPlayer = game.Players.LocalPlayer
