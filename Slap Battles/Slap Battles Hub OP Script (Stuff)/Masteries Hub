local brickMasterRunning = false
local brickMasterConnection = nil

Tab:AddToggle({
    Name = "Auto Obtain Brick Master Badge",
    Default = false,
    Callback = function(Value)
        if Value then
            -- START
            if game.Players.LocalPlayer.leaderstats.Glove.Value == "Brick" and game.Players.LocalPlayer.Character:FindFirstChild("entered") then
                task.wait(0.5)
                local player = game.Players.LocalPlayer

                if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                    task.wait(0.5)
                    player.Character.HumanoidRootPart.CFrame = CFrame.new(17894, -24, -3533)
                end

                task.wait(0.5)
                
                -- Create the loop
                getgenv().BrickMasterLoop = true
                task.spawn(function()
                    while getgenv().BrickMasterLoop do
                        game:GetService("ReplicatedStorage").lbrick:FireServer()
                        local gui = game:GetService("Players").LocalPlayer.PlayerGui.BRICKCOUNT.ImageLabel.TextLabel
                        gui.Text = tonumber(gui.Text) + 1
                        task.wait(1.5)
                    end
                end)
                
            else
                OrionLib:MakeNotification({
                    Name = "Error",
                    Content = "Equip Brick Glove/Enter Arena",
                    Image = "rbxassetid://7743878857",
                    Time = 3
                })
                return false -- Turn toggle off
            end
        else
            -- STOP
            getgenv().BrickMasterLoop = false
            
        end
    end    
})

Section = Tab:AddSection({
    Name = "3 Quest"
})

Tab:AddToggle({
    Name = "Auto Fling Bricks (can lag)",
    Default = false,
    Callback = function(Value)
        if Value then
            -- START
            if game.Players.LocalPlayer.leaderstats.Glove.Value == "Brick" then
                
                getgenv().UnionsFlingLoop = true
                
                -- Store original walk speed
                local originalWalkSpeed = game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Humanoid").WalkSpeed
                
                task.spawn(function()
                    -- Infinite Yield style fling loop
                    local RunService = game:GetService("RunService")
                    local speaker = game.Players.LocalPlayer
                    local flinging = true
                    
                    -- Noclip for flinging
                    local noclipConnection
                    noclipConnection = RunService.Stepped:Connect(function()
                        if speaker.Character then
                            for _, part in pairs(speaker.Character:GetDescendants()) do
                                if part:IsA("BasePart") and part.CanCollide then
                                    part.CanCollide = false
                                end
                            end
                        end
                    end)
                    
                    -- Death handler
                    local humanoid = speaker.Character:FindFirstChildWhichIsA("Humanoid")
                    if humanoid then
                        humanoid.Died:Connect(function()
                            flinging = false
                            if noclipConnection then
                                noclipConnection:Disconnect()
                            end
                            getgenv().UnionsFlingLoop = false
                        end)
                    end
                    
                    while getgenv().UnionsFlingLoop and flinging do
                        RunService.Heartbeat:Wait()
                        local character = speaker.Character
                        local root = character and character:FindFirstChild("HumanoidRootPart")
                        local vel, movel = nil, 0.1

                        while not (character and character.Parent and root and root.Parent) do
                            RunService.Heartbeat:Wait()
                            character = speaker.Character
                            root = character and character:FindFirstChild("HumanoidRootPart")
                        end

                        vel = root.Velocity
                        root.Velocity = vel * 10000 + Vector3.new(0, 10000, 0)

                        RunService.RenderStepped:Wait()
                        if character and character.Parent and root and root.Parent then
                            root.Velocity = vel
                        end

                        RunService.Stepped:Wait()
                        if character and character.Parent and root and root.Parent then
                            root.Velocity = vel + Vector3.new(0, movel, 0)
                            movel = movel * -1
                        end
                        
                        -- Find and bring Unions with TouchInterest to player while flinging
                        local unions = {}
                        for _, obj in pairs(workspace:GetDescendants()) do
                            if obj:IsA("BasePart") and obj.Name == "Union" then
                                -- Check if this Union has TouchInterest inside
                                local hasTouchInterest = false
                                for _, child in pairs(obj:GetChildren()) do
                                    if child:IsA("TouchTransmitter") or child.Name == "TouchInterest" then
                                        hasTouchInterest = true
                                        break
                                    end
                                end
                                
                                if hasTouchInterest then
                                    table.insert(unions, obj)
                                end
                            end
                        end
                    end
                    
                    -- Cleanup connections when loop ends
                    if noclipConnection then
                        noclipConnection:Disconnect()
                    end
                end)
            else
                OrionLib:MakeNotification({
                    Name = "Error",
                    Content = "Equip Brick Glove first!",
                    Image = "rbxassetid://7743878857",
                    Time = 3
                })
                return false -- Turn toggle off
            end
        else
            -- STOP
            getgenv().UnionsFlingLoop = false
            
            -- Restore original walk speed
            if game.Players.LocalPlayer.Character then
                local humanoid = game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Humanoid")
                if humanoid then
                    humanoid.WalkSpeed = 16 -- Default walk speed
                end
            end
            
            -- Clean up velocity objects from Unions
            for _, obj in pairs(workspace:GetDescendants()) do
                if obj:IsA("BasePart") and obj.Name == "Union" then
                    local velocity = obj:FindFirstChild("FlingVelocity")
                    if velocity then
                        velocity:Destroy()
                    end
                end
            end
            
        end
    end    
})

