local Tab = Window:MakeTab({
    Name = "Phase",
    Icon = "rbxassetid://7734068321",
    PremiumOnly = false
})

Section = Tab:AddSection({
    Name = "1-3 Quest"
})

local AutoAbiltySlapPhaseRunning = false

Tab:AddToggle({
    Name = "Auto Ability + Slap People",
    Default = false,
    Callback = function(Value)
        AutoAbiltySlapPhaseRunning = Value
        local player = game.Players.LocalPlayer

        if AutoAbiltySlapPhaseRunning then
            if player.leaderstats.Glove.Value == "Phase" then
                task.spawn(function()
                    while AutoAbiltySlapPhaseRunning do
                        local players = game:GetService("Players"):GetPlayers()
                        local targetPlayer

                        repeat
                            targetPlayer = players[math.random(1, #players)]
                        until targetPlayer ~= player and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart")

                        if targetPlayer and targetPlayer.Character then
                            player.Character.HumanoidRootPart.CFrame = CFrame.new(targetPlayer.Character.HumanoidRootPart.Position)
                        end

                        task.wait(1)
                    end
                end)

                task.spawn(function()
                    while true do
                        game:GetService("ReplicatedStorage").PhaseA:FireServer()
                        task.wait(14)
                    end
                end)
            else
                AutoAbiltySlapPhaseRunning = false
                OrionLib:MakeNotification({
                    Name = "Error",
                    Content = "Equip Phase Glove",
                    Image = "rbxassetid://7743878857",
                    Time = 3
                })
            end
        end
    end
})

Section = Tab:AddSection({
    Name = "2 Quest"
})

Tab:AddButton({
    Name = "Auto Ability + Teleport To All Islands (Takes 112 Seconds)",
    Callback = function()
        local player = game.Players.LocalPlayer

        if player.leaderstats.Glove.Value == "Phase" and player.Character:FindFirstChild("entered") then
            local function phaseTo(position)
                player.Character.HumanoidRootPart.CFrame = CFrame.new(122, 255, 3)
                task.wait(0.1)
                game:GetService("ReplicatedStorage").PhaseA:FireServer()
                task.wait(0.1)
                player.Character.HumanoidRootPart.CFrame = CFrame.new(position)
                task.wait(14)
            end

            phaseTo(Vector3.new(236, -16, -6))
            phaseTo(Vector3.new()) -- likely needs correction
            phaseTo(Vector3.new(4, -5, -223))
            phaseTo(Vector3.new(-216, -5, -6))
            phaseTo(Vector3.new(-401, 51, -16))
            phaseTo(Vector3.new(256, 34, 194))
            phaseTo(Vector3.new(4, -5, 215))
            phaseTo(Vector3.new(-125, -5, 122))
        else
            OrionLib:MakeNotification({
                Name = "Error",
                Content = "Equip Phase Glove/Use In Arena",
                Image = "rbxassetid://7743878857",
                Time = 3
            })
        end
    end
})

local Tab = Window:MakeTab({
    Name = "Bomb",
    Icon = "rbxassetid://7734068321",
    PremiumOnly = false
})

Section = Tab:AddSection({
    Name = "1-2-3 Quest"
})

Tab:AddButton({
    Name = "Throw Bomb/Explode Bomb",
    Callback = function()
        if game.Players.LocalPlayer.leaderstats.Glove.Value == "Bomb" then
            game:GetService("ReplicatedStorage").BombThrow:FireServer("Ebutton")
        else
            OrionLib:MakeNotification({
                Name = "Error",
                Content = "Equip Bomb",
                Image = "rbxassetid://7743878857",
                Time = 3
            })
        end
    end
})

Tab:AddToggle({
    Name = "Auto Do 1-2-3 Tasks",
    Default = false,
    Callback = function(Value)
        if Value then
            -- START
            if game.Players.LocalPlayer.leaderstats.Glove.Value == "Bomb" then
                
                getgenv().AutoBombTasksLoop = true
                task.spawn(function()
                    while getgenv().AutoBombTasksLoop do
                        local player = game.Players.LocalPlayer

                        -- Check if character exists
                        if not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") then
                            task.wait(1)
                            continue
                        end

                        -- Move to safe area
                        player.Character.HumanoidRootPart.CFrame = CFrame.new(-908, 328, 5)
                        task.wait(1)

                        -- Move to platform
                        player.Character.HumanoidRootPart.CFrame = CFrame.new(10019, -42, 9960)
                        task.wait(1)

                        -- Throw Bomb
                        game:GetService("ReplicatedStorage").BombThrow:FireServer("Ebutton")
                        task.wait(1)

                        -- Wait to teleport to bomb
                        local bombFound = false
                        for i = 1, 30 do -- Timeout after 30 seconds
                            if not getgenv().AutoBombTasksLoop then break end
                            
                            for _, v in pairs(game.Workspace:GetChildren()) do
                                if string.find(v.Name, "_bÃ¸mb") then
                                    player.Character.HumanoidRootPart.CFrame = v.CFrame
                                    bombFound = true
                                    break
                                end
                            end
                            
                            if bombFound then
                                break
                            else
                                task.wait(1)
                            end
                        end

                        task.wait(9)
                        
                        -- Kill player
                        local humanoid = player.Character:FindFirstChild("Humanoid")
                        if humanoid then
                            humanoid.Health = 0
                        end
                        
                        task.wait(23)
                    end
                end)
            else
                OrionLib:MakeNotification({
                    Name = "Error",
                    Content = "Equip Bomb Glove",
                    Image = "rbxassetid://7743878857",
                    Time = 3
                })
                return false -- Turn toggle off
            end
        else
            -- STOP
            getgenv().AutoBombTasksLoop = false
            
        end
    end
})

