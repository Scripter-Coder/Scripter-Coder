Tab:AddToggle({
    Name = "Auto Spawn Frenzy Bots",
    Default = false,
    Callback = function(Value)
        if Value then
            -- START
            
            getgenv().AutoFrenzyLoop = true
            task.spawn(function()
                while getgenv().AutoFrenzyLoop do
                    game:GetService("ReplicatedStorage").GeneralAbility:FireServer()
                    task.wait(7)
                end
            end)
        else
            -- STOP
            getgenv().AutoFrenzyLoop = false
            
        end
    end
