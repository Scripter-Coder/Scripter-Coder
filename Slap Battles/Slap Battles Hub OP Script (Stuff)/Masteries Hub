local running = false -- declare global toggle state

Tab:AddToggle({
    Name = "Auto Use Shard's Glove Ability",
    Default = false,
    Callback = function(Value)
        if Value then
            -- Start the loop with exact 0.1 second interval
            shardCloneLoop = true
            
            spawn(function()
                while cherryCloneLoop do
                    local player = game.Players.LocalPlayer
                    
                    -- Check if player has Cherry glove
                    if player.leaderstats and player.leaderstats.Glove and player.leaderstats.Glove.Value == "Shard" then
                        -- Check if player has entered
                        if player.Character and player.Character:FindFirstChild("entered") then
                            -- Fire the Cherry remote
                            game:GetService("ReplicatedStorage").Events.Shards:FireServer()
                        else
                            OrionLib:MakeNotification({
                                Name = "Not Entered",
                                Content = "You need to enter the arena first!",
                                Image = "rbxassetid://4483345998",
                                Time = 3
                            })
                            wait(1) -- Wait longer when not entered
                        end
                    else
                        OrionLib:MakeNotification({
                            Name = "Wrong Glove",
                            Content = "You need Shard glove equipped!",
                            Image = "rbxassetid://4483345998",
                            Time = 3
                        })
                        wait(1) -- Wait longer when wrong glove
                    end
                    
                    wait(0.1) -- Normal 0.1 second delay
                end
            end)
        else
            -- Stop the loop
            shardCloneLoop = false
        end
    end
})

Tab:AddButton({
    Name = "Shard Slap Aura",
    Callback = function()
        while true do
            local players = game:GetService("Players")
            local replicatedStorage = game:GetService("ReplicatedStorage")

            for _, player in ipairs(players:GetPlayers()) do
                local character = player.Character
                if character then
                    local leftArm = character:FindFirstChild("Left Arm")
                    if leftArm then
                        replicatedStorage.ShardHIT:FireServer(leftArm)
                    end
                end
            end

            wait(0.5)
        end
    end
})

local Tab = Window:MakeTab({
    Name = "Moon",
    Icon = "rbxassetid://7734068321",
    PremiumOnly = false
})

Section = Tab:AddSection({
    Name = "1-2 Quest"
})

Tab:AddToggle({
    Name = "Auto Ragdoll + Touch Ground",
    Default = false,
    Callback = function(Value)
        if Value then
            -- Start the loop
            autoRagdollLoop = game:GetService("RunService").Heartbeat:Connect(function()
                -- Check if player has entered
                if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("entered") then
                    -- Fire knockback remote
                    game.ReplicatedStorage.SelfKnockback:FireServer({
                        ["Force"] = 0,
                        ["Direction"] = Vector3.new(0, 0.01, 0)
                    })
                    
                    -- Teleport to position
                    if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1, -5, 8)
                    end
                end
            end)
        else
            -- Stop the loop
            if autoRagdollLoop then
                autoRagdollLoop:Disconnect()
                autoRagdollLoop = nil
            end
        end
    end
})

Tab:AddToggle({
    Name = "Auto Spam Equip/Unequip Moon Glove",
    Default = false,
    Callback = function(Value)
        if Value then
            -- Start the loop
            moonGloveLoop = game:GetService("RunService").Heartbeat:Connect(function()
                local player = game.Players.LocalPlayer
                local backpack = player:FindFirstChild("Backpack")
                
                if backpack then
                    local moonTool = backpack:FindFirstChild("Moon")
                    
                    if moonTool then
                        -- Check if tool is currently equipped
                        local character = player.Character
                        local currentlyEquipped = character and character:FindFirstChild("Moon")
                        
                        if currentlyEquipped then
                            -- Unequip: move tool back to backpack
                            moonTool.Parent = backpack
                        else
                            -- Equip: move tool to character
                            moonTool.Parent = character
                        end
                    end
                end
            end)
        else
            -- Stop the loop
            if moonGloveLoop then
                moonGloveLoop:Disconnect()
                moonGloveLoop = nil
            end
        end
    end
})

