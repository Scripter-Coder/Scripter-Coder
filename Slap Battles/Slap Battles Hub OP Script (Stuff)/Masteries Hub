    Name = "WalkSpeed",
    Default = "UserSpeed",
    TextDisappear = false,
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
        Walkspeed = Value
    end
})

Tab:AddToggle({
    Name = "Walkspeed Set Auto",
    Default = false,
    Callback = function(Value)
        KeepWalkspeed = Value

        while KeepWalkspeed do
            if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and
               game.Players.LocalPlayer.Character.Humanoid.WalkSpeed ~= Walkspeed then
                game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Walkspeed
            end
            task.wait()
        end
    end
})

Section = Tab:AddSection({
    Name = "3 Quest"
})

local AutoAbsorb = false

Tab:AddToggle({
    Name = "Auto Absorb People",
    Default = false,
    Callback = function(Value)
        AutoAbsorb = Value
        local player = game.Players.LocalPlayer

        if AutoAbsorb then
            if player.leaderstats.Glove.Value:lower() == "rob" then
                task.spawn(function()
                    while AutoAbsorb do
                        local players = game:GetService("Players"):GetPlayers()
                        local target

                        repeat
                            target = players[math.random(1, #players)]
                        until target ~= player and target.Character and target.Character:FindFirstChild("HumanoidRootPart")

                        local targetHRP = target.Character:FindFirstChild("HumanoidRootPart")
                        if targetHRP and player.Character:FindFirstChild("HumanoidRootPart") then
                            player.Character.HumanoidRootPart.CFrame = CFrame.new(targetHRP.Position + Vector3.new(0, 0, 2))
                        end

                        game:GetService("ReplicatedStorage"):FindFirstChild("Ability"):FireServer()
                        task.wait(1)
                    end
                end)
            else
                AutoAbsorb = false
                OrionLib:MakeNotification({
                    Name = "Error",
                    Content = "Equip Rob Glove",
                    Image = "rbxassetid://7743878857",
                    Time = 3
                })
            end
        end
    end
})

Tab:AddToggle({
    Name = "Auto Rob Transformation",
    Default = false,
    Callback = function(Value)
        if Value then
            -- START
            
            getgenv().AutoRobLoop = true
            task.spawn(function()
                while getgenv().AutoRobLoop do
                    -- Check if the ClickDetector exists
                    local robObject = game:GetService("Workspace"):FindFirstChild("Lobby")
                    if robObject then
                        robObject = robObject:FindFirstChild("rob")
                        if robObject and robObject:FindFirstChild("ClickDetector") then
                            fireclickdetector(robObject.ClickDetector)
                        end
                    end
                    
                    task.wait(0.5)
                    
                    -- Fire the remote
                    game:GetService("ReplicatedStorage").rob:FireServer()
                    
                    task.wait(17)
                end
            end)
        else
            -- STOP
            getgenv().AutoRobLoop = false
            
        end
    end
})

