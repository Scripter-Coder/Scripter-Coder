local Tab = Window:MakeTab({
    Name = "Plank",
    Icon = "rbxassetid://7734068321",
    PremiumOnly = false
})

Section = Tab:AddSection({
    Name = "1-2-3 Quests"
})

local AutoPlankToggle = Tab:AddToggle({
    Name = "Auto Use Plank On Players",
    Default = false,
    Callback = function(Value)
        _G.AutoPlank = Value
        
        -- Main loop for auto plank
        while _G.AutoPlank and task.wait(0.1) do
            -- Get all players except yourself
            local players = game:GetService("Players"):GetPlayers()
            local targetPlayers = {}
            
            for _, player in ipairs(players) do
                if player ~= game:GetService("Players").LocalPlayer and player.Character then
                    -- Get the humanoid root part of the target player
                    local hrp = player.Character:FindFirstChild("HumanoidRootPart")
                    if hrp then
                        table.insert(targetPlayers, {
                            player = player,
                            position = hrp.Position
                        })
                    end
                end
            end
            
            -- If there are valid players, pick a random one
            if #targetPlayers > 0 then
                local randomTarget = targetPlayers[math.random(1, #targetPlayers)]
                
                -- Create the args with the target player's position
                local args = {
                    [1] = "OutOfBody",
                    [2] = {
                        ["mousebehavior"] = Enum.MouseBehavior.Default,
                        ["ismoving"] = false,
                        ["dir"] = Vector3.new(
                            randomTarget.position.X,
                            randomTarget.position.Y,
                            randomTarget.position.Z
                        )
                    }
                }
                
                -- Fire the remote
                game:GetService("ReplicatedStorage").Blink:FireServer(unpack(args))
            end
        end
    end
})

local Tab = Window:MakeTab({
    Name = "Car Keys",
    Icon = "rbxassetid://7734068321",
    PremiumOnly = false
})

Section = Tab:AddSection({
    Name = "1 Quest"
})

local toggleOn = false
local upgradeLoop = nil

local attributeFrames = {
    "UtilityAttributeFrame",
    "ArmorAttributeFrame",
    "ReverseAttributeFrame",
    "SteerAngleAttributeFrame",
    "AccelerationAttributeFrame",
    "MaxSpeedAttributeFrame",
    "KnockbackAttributeFrame"
}

Tab:AddToggle({
    Name = "Auto Upgrade All Stats",
    Default = false,
    Callback = function(Value)
        toggleOn = Value
        if toggleOn then
            -- Start upgrade loop
            upgradeLoop = task.spawn(function()
                while toggleOn do
                    for _, attr in ipairs(attributeFrames) do
                        local args = { attr }
                        game:GetService("ReplicatedStorage"):WaitForChild("CarKeysUpgrade"):FireServer(unpack(args))
                        task.wait(0.1) -- small delay to prevent spamming too fast
                    end
                    task.wait(0.5) -- wait between full upgrade cycles
                end
            end)
        else
            -- Stop loop by breaking the while condition
            toggleOn = false
        end
    end
})

Section = Tab:AddSection({
    Name = "2-3 Quest"
})

Tab:AddButton({
    Name = "Car Keys Slap Aura",
    Callback = function()
if game.Players.LocalPlayer.leaderstats.Glove.Value == "Car Keys" then
-- Get the ReplicatedStorage and Players service
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Players = game:GetService("Players")

-- Start the infinite loop to fire for all players
while true do
    -- Loop through each player in the game
    for _, player in pairs(Players:GetPlayers()) do
        -- Ensure the player has a character and it contains the "Right Arm"
        if player.Character and player.Character:FindFirstChild("Right Arm") then
            -- Create the arguments table for the FireServer function
            local args = {
                [1] = player.Character["Right Arm"]
            }

            -- Fire the server with the "Right Arm" part as an argument
            ReplicatedStorage.GeneralHit:FireServer(unpack(args))
        end
    end

    -- Wait 1 second before running the loop again (adjust the wait time as needed)
    task.wait(0.5)
end
else
OrionLib:MakeNotification({Name = "Error",Content = "Equip Car Keys",Image = "rbxassetid://7743878857",Time = 3})
end
end
})

local Tab = Window:MakeTab({
    Name = "Ultra Instinct",
    Icon = "rbxassetid://7734068321",
    PremiumOnly = false
})

Section = Tab:AddSection({
    Name = "1-2 Quest"
})

local running = false -- declare global toggle state

Tab:AddToggle({
    Name = "Auto Slap Players",
    Default = false,
    Callback = function(AutoSlapVoodoo)
        running = AutoSlapVoodoo

        if not running then
            return -- stop immediately if toggled off
        end

        -- Run everything in a separate thread
        task.spawn(function()
            local Players = game:GetService("Players")
            local ReplicatedStorage = game:GetService("ReplicatedStorage")
            local LocalPlayer = Players.LocalPlayer

            -- Function to teleport to a random player
            local function teleportToRandomPlayer()
                local playerList = Players:GetPlayers()
                if #playerList > 1 then
                    local randomPlayer = nil
                    repeat
                        randomPlayer = playerList[math.random(1, #playerList)]
                    until randomPlayer ~= LocalPlayer

                    local targetChar = randomPlayer.Character
                    if targetChar and targetChar:FindFirstChild("HumanoidRootPart") then
                        LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(targetChar.HumanoidRootPart.Position + Vector3.new(0, 3, 0))
                    end
                end
            end

            -- Function to fire GeneralHit for all players
            local function fireGeneralHitForAllPlayers()
                for _, player in pairs(Players:GetPlayers()) do
                    if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                        ReplicatedStorage:WaitForChild("GeneralHit"):FireServer(player.Character.HumanoidRootPart)
                    end
                end
            end

            -- Loop 750 times or until stopped
            local count = 0
            while running and count < 750 do
                if LocalPlayer.leaderstats.Glove.Value == "Ultra Instinct" then
                    teleportToRandomPlayer()
                    fireGeneralHitForAllPlayers()
                    count += 1
                    task.wait(1)
                else
                    OrionLib:MakeNotification({
                        Name = "Error",
                        Content = "Equip Ultra Instinct Glove",
                        Image = "rbxassetid://7743878857",
                        Time = 3
                    })
                    break
                end
            end
        end)
    end
})

Tab:AddButton({
    Name = "Teleport to Random Player",
    Callback = function()
        local players = {}
        local localPlayer = game.Players.LocalPlayer
        
        -- Get all players except yourself
        for _, player in pairs(game.Players:GetPlayers()) do
            if player ~= localPlayer and player.Character then
                local hrp = player.Character:FindFirstChild("HumanoidRootPart")
                if hrp then
                    table.insert(players, player)
                end
            end
        end
        
        if #players > 0 then
            -- Select random player
            local randomPlayer = players[math.random(1, #players)]
            local targetHRP = randomPlayer.Character.HumanoidRootPart
            
            -- Teleport to the random player
            if localPlayer.Character and localPlayer.Character:FindFirstChild("HumanoidRootPart") then
                localPlayer.Character.HumanoidRootPart.CFrame = targetHRP.CFrame
                
            end
        else

        end
    end
})

Tab:AddToggle({
    Name = "Auto Teleport to Random Players",
    Default = false,
    Callback = function(Value)
        if Value then
            -- START
            
            getgenv().AutoTeleportPlayersLoop = true
            task.spawn(function()
                while getgenv().AutoTeleportPlayersLoop do
                    local localPlayer = game.Players.LocalPlayer
                    local players = {}
                    
                    -- Get all players except yourself
                    for _, player in pairs(game.Players:GetPlayers()) do
                        if player ~= localPlayer and player.Character then
                            local hrp = player.Character:FindFirstChild("HumanoidRootPart")
                            if hrp then
                                table.insert(players, player)
                            end
                        end
                    end
                    
                    if #players > 0 and localPlayer.Character and localPlayer.Character:FindFirstChild("HumanoidRootPart") then
                        -- Select random player
                        local randomPlayer = players[math.random(1, #players)]
                        local targetHRP = randomPlayer.Character.HumanoidRootPart
                        
                        -- Teleport to the random player
                        localPlayer.Character.HumanoidRootPart.CFrame = targetHRP.CFrame
                    end
                    
                    task.wait(0.1) -- Wait 0.1 seconds as requested
                end
            end)
        else
            -- STOP
            getgenv().AutoTeleportPlayersLoop = false
            
        end
    end
})

