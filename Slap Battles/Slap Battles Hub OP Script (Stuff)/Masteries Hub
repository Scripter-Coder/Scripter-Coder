Tab:AddButton({
    Name = "Create Null Portal (Need Bob And Rob Gloves)",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new()
        wait(0.5)

        fireclickdetector(game:GetService("Workspace").Lobby.rob.ClickDetector)
        wait(0.5)
        game:GetService("ReplicatedStorage").rob:FireServer()
        wait(0.5)

        fireclickdetector(game:GetService("Workspace").Lobby.bob.ClickDetector)
        wait(0.5)
        game:GetService("ReplicatedStorage").bob:FireServer()

        OrionLib:MakeNotification({
            Name = "Error",
            Content = "Need Bob/Rob Badges To Own",
            Image = "rbxassetid://7743878857",
            Time = 3
        })
    end
})

