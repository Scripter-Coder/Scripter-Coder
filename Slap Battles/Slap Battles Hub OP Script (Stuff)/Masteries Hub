Section = Tab:AddSection({
    Name = "2 Quest"
})

local running = false

Tab:AddToggle({
    Name = "Auto Slap/Use Ability 750 Times",
    Default = false,
    Callback = function(AutoSlapFlash)
        running = AutoSlapFlash
        if not running then return end

        task.spawn(function()
            local Players = game:GetService("Players")
            local ReplicatedStorage = game:GetService("ReplicatedStorage")
            local LocalPlayer = Players.LocalPlayer

            local function teleportToRandomPlayer()
                local playerList = Players:GetPlayers()
                if #playerList > 1 then
                    local randomPlayer
                    repeat
                        randomPlayer = playerList[math.random(1, #playerList)]
                    until randomPlayer ~= LocalPlayer

                    local targetChar = randomPlayer.Character
                    if targetChar and targetChar:FindFirstChild("HumanoidRootPart") then
                        LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(targetChar.HumanoidRootPart.Position + Vector3.new(0, 3, 0))
                    end
                end
            end

            local function fireFlashHitForAllPlayers()
                for _, player in pairs(Players:GetPlayers()) do
                    if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                        ReplicatedStorage:WaitForChild("FlashHit"):FireServer(player.Character.HumanoidRootPart)
                    end
                end
            end

            local count = 0
            while running and count < 750 do
                if LocalPlayer.leaderstats.Glove.Value == "Flash" then
                    ReplicatedStorage:WaitForChild("FlashTeleport"):FireServer()
                    teleportToRandomPlayer()
                    fireFlashHitForAllPlayers()
                    count += 1
                    task.wait(2.1)
                else
                    OrionLib:MakeNotification({
                        Name = "Error",
                        Content = "Equip Flash Glove",
                        Image = "rbxassetid://7743878857",
                        Time = 3
                    })
                    break
                end
            end
        end)
    end
})

