-- Status label
local statusLabel = DualBotSection:AddLabel("Status: Ready")

-- Update status periodically
spawn(function()
    while true do
        task.wait(1)
        if dualBotActive then
            if isDualEquipped() then
                if amIInArena() then
                    local target = getClosestPlayer()
                    if target then
                        local distance = 0
                        if character and humanoidRootPart and target.Character then
                            local targetHRP = target.Character:FindFirstChild("HumanoidRootPart")
                            if targetHRP then
                                distance = (humanoidRootPart.Position - targetHRP.Position).Magnitude
                            end
                        end
                        statusLabel:Set("Status: Active | Target: " .. target.Name .. " (" .. math.floor(distance) .. " studs)")
                    else
                        statusLabel:Set("Status: Active | No players in arena")
                    end
                else
                    statusLabel:Set("Status: NOT IN ARENA - Enter arena!")
                end
            else
                statusLabel:Set("Status: ERROR - Equip Dual glove!")
            end
        else
            statusLabel:Set("Status: Ready")
        end
    end
end)

