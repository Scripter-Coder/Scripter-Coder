                    Name = "Error",
                    Content = "Equip Dual glove first!",
                    Image = "rbxassetid://7743878857",
                    Time = 3
                })
                return false
            end
            
            dualBotActive = true
            task.spawn(startDualBot)
        else
            dualBotActive = false
        end
    end
})

