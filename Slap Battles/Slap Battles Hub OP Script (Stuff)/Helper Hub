-- Username input and help buttons
local targetUsername = "" -- Store the username

-- Create textbox for username input
Tab:AddTextbox({
    Name = "Player Username",
    Default = "",
    TextDisappear = false, -- Text won't disappear
    Callback = function(Value)
        targetUsername = Value
        if Value ~= "" then
            OrionLib:MakeNotification({
                Name = "Target Set",
                Content = "Targeting player: " .. Value,
                Image = "rbxassetid://4483345998",
                Time = 3
            })
        end
    end
})

-- Help Player Get Duck button
Tab:AddButton({
    Name = "Help Player Get Duck",
    Callback = function()
        if targetUsername == "" then
            OrionLib:MakeNotification({
                Name = "Error",
                Content = "Enter a username first!",
                Image = "rbxassetid://7743878857",
                Time = 3
            })
            return
        end
        
        -- Send message to help player find duck
        local message = "@" .. targetUsername .. " Duck Is Located In Default Island Under Tree"
        
        -- Get chat service
        local ChatService = game:GetService("ReplicatedStorage"):FindFirstChild("DefaultChatSystemChatEvents")
        if ChatService then
            local SayMessageRequest = ChatService:FindFirstChild("SayMessageRequest")
            if SayMessageRequest then
                SayMessageRequest:FireServer(message, "All")
                OrionLib:MakeNotification({
                    Name = "Message Sent",
                    Content = "Sent duck location to " .. targetUsername,
                    Image = "rbxassetid://4483345998",
                    Time = 3
                })
            else
                OrionLib:MakeNotification({
                    Name = "Error",
                    Content = "Chat system not found!",
                    Image = "rbxassetid://7743878857",
                    Time = 3
                })
            end
        else
            -- Alternative method using text chat service
            local TextChatService = game:GetService("TextChatService")
            if TextChatService then
                local channels = TextChatService:FindFirstChild("TextChannels")
                if channels then
                    local rbxts = channels:FindFirstChild("RBXGeneral")
                    if rbxts then
                        rbxts:SendAsync(message)
                        OrionLib:MakeNotification({
                            Name = "Message Sent",
                            Content = "Sent duck location to " .. targetUsername,
                            Image = "rbxassetid://4483345998",
                            Time = 3
                        })
                    end
                end
            end
        end
    end
})

