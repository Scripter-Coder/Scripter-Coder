-- Teleport to Specific Player System
local teleportTarget = ""

-- Textbox for username input
Tab:AddTextbox({
    Name = "Player Username To Help",
    Default = "",
    TextDisappear = false,
    Callback = function(Value)
        teleportTarget = Value
        if Value ~= "" then
            OrionLib:MakeNotification({
                Name = "Target Set",
                Content = "Searching for: " .. Value,
                Image = "rbxassetid://4483345998",
                Time = 2
            })
        end
    end
})

-- Function to find player by partial or full username
local function findPlayerByName(username)
    if username == "" then return nil end
    
    username = string.lower(username) -- Make search case-insensitive
    
    local players = game:GetService("Players"):GetPlayers()
    
    -- First try exact match
    for _, player in ipairs(players) do
        if string.lower(player.Name) == username then
            return player
        end
    end
    
    -- Then try partial match
    for _, player in ipairs(players) do
        if string.lower(player.Name):find(username, 1, true) then
            return player
        end
    end
    
    -- Try display name as well
    for _, player in ipairs(players) do
        if string.lower(player.DisplayName):find(username, 1, true) then
            return player
        end
    end
    
    return nil
end

-- Function to teleport to player
local function teleportToPlayer(player)
    if not player then
        OrionLib:MakeNotification({
            Name = "Error",
            Content = "Player not found!",
            Image = "rbxassetid://7743878857",
            Time = 3
        })
        return false
    end
    
    local localPlayer = game.Players.LocalPlayer
    local localCharacter = localPlayer.Character
    local targetCharacter = player.Character
    
    if not localCharacter then
        OrionLib:MakeNotification({
            Name = "Error",
            Content = "Your character not loaded!",
            Image = "rbxassetid://7743878857",
            Time = 3
        })
        return false
    end
    
    if not targetCharacter then
        OrionLib:MakeNotification({
            Name = "Error",
            Content = player.Name .. "'s character not loaded!",
            Image = "rbxassetid://7743878857",
            Time = 3
        })
        return false
    end
    
    -- Find HumanoidRootPart or Torso
    local targetHRP = targetCharacter:FindFirstChild("HumanoidRootPart") or targetCharacter:FindFirstChild("Torso")
    local localHRP = localCharacter:FindFirstChild("HumanoidRootPart") or localCharacter:FindFirstChild("Torso")
    
    if not targetHRP then
        OrionLib:MakeNotification({
            Name = "Error",
            Content = player.Name .. " has no HumanoidRootPart!",
            Image = "rbxassetid://7743878857",
            Time = 3
        })
        return false
    end
    
    if not localHRP then
        OrionLib:MakeNotification({
            Name = "Error",
            Content = "You have no HumanoidRootPart!",
            Image = "rbxassetid://7743878857",
            Time = 3
        })
        return false
    end
    
    -- Check if target has Humanoid (alive check)
    local targetHumanoid = targetCharacter:FindFirstChildOfClass("Humanoid")
    if targetHumanoid and targetHumanoid.Health <= 0 then
        OrionLib:MakeNotification({
            Name = "Warning",
            Content = player.Name .. " is dead!",
            Image = "rbxassetid://7743878857",
            Time = 3
        })
    end
    
    -- Teleport to player
    local success = pcall(function()
        -- Teleport slightly above to avoid collisions
        local teleportPos = targetHRP.Position + Vector3.new(0, 3, 0)
        localHRP.CFrame = CFrame.new(teleportPos)
    end)
    
    if success then
        OrionLib:MakeNotification({
            Name = "Success",
            Content = "Teleported to " .. player.Name,
            Image = "rbxassetid://4483345998",
            Time = 3
        })
        return true
    else
        OrionLib:MakeNotification({
            Name = "Error",
            Content = "Failed to teleport!",
            Image = "rbxassetid://7743878857",
            Time = 3
        })
        return false
    end
end

-- Teleport button
Tab:AddButton({
    Name = "Teleport to Player",
    Callback = function()
        if teleportTarget == "" then
            OrionLib:MakeNotification({
                Name = "Error",
                Content = "Enter username first!",
                Image = "rbxassetid://7743878857",
                Time = 3
            })
            return
        end
        
        local player = findPlayerByName(teleportTarget)
        teleportToPlayer(player)
    end
})

