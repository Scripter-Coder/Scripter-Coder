Tab:AddToggle({
    Name = "Auto Farm (Jet/Phase/Glitch/Siphon/Materialize/Slapple/Toolbox)",
    Default = false,
    Callback = function(Value)
        _G.PhaseOrJetfarm = Value

        while _G.PhaseOrJetfarm do
            for _, v in pairs(game.Workspace:GetChildren()) do
                if v.Name == "JetOrb"
                or v.Name == "PhaseOrb"
                or v.Name == "GlitchOrb"
                or v.Name == "SiphonOrb"
                or v.Name == "MaterializeOrb"
                or v.Name == "Slapple"
                or v.Name == "GoldenSlapple"
                or v.Name == "Toolbox" then
                    local head = game.Players.LocalPlayer.Character:WaitForChild("Head")
                    firetouchinterest(head, v, 0)
                    firetouchinterest(head, v, 1)
                end
            end
            task.wait()
        end
    end
})

