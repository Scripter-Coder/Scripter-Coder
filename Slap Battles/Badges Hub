				task.spawn(function()
					while Potatofarm do
						for _, v in pairs(game.Workspace:GetChildren()) do
							if v.Name == "BubbleObject" then
								local hrp = player.Character:FindFirstChild("HumanoidRootPart")
								if hrp then
									firetouchinterest(hrp, v, 0)
									firetouchinterest(hrp, v, 1)
								end
							end
						end
						task.wait(0.1) -- avoid tight looping
					end
				end)
			else
OrionLib:MakeNotification({Name = "Error",Content = "Equip Reverse/Use Ability",Image = "rbxassetid://7743878857",Time = 3})
			end
		end
