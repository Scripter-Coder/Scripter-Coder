Tab:AddButton({
    Name = "Someone Using woah?",
    Callback = function()
        local found = false

        for _, player in ipairs(game.Players:GetPlayers()) do
            local leaderstats = player:FindFirstChild("leaderstats")
            if leaderstats then
                local glove = leaderstats:FindFirstChild("Glove")
                if glove and glove.Value == "woah" then
                    found = true
                    OrionLib:MakeNotification({
                        Name = "Detected!",
                        Content = player.Name .. " has equipped the woah glove.",
                        Image = "rbxassetid://7733715400",
                        Time = 3
                    })
                end
            end
        end

        if not found then
            OrionLib:MakeNotification({
                Name = "Error",
                Content = "No one has equipped the woah glove",
                Image = "rbxassetid://7743878857",
                Time = 3
            })
        end
    end
})

