-- Harvester Button with Auto-Execute
Tab:AddButton({
    Name = "Auto Teleport To Pim Corn Place + Get Harvester",
    Callback = function()
        local firstPlaceId = 6403373529
        local secondPlaceId = 92483185275059
        local currentPlaceId = game.PlaceId
        
        local function executeHarvesterScript()
            if not game:IsLoaded() then game.Loaded:Wait() end
            if getgenv().AutoGetHarvesterExecuted then return end
            getgenv().AutoGetHarvesterExecuted = true

            if game.PlaceId == secondPlaceId then
                -- Second place: Pim Corn Place - Load the harvester script
                loadstring(game:HttpGet("https://raw.githubusercontent.com/IncognitoScripts/SlapBattles/main/AutoHarvester", true))()
                
            elseif game.PlaceId == firstPlaceId or game.PlaceId == 11520107397 or game.PlaceId == 9015014224 or game.PlaceId == 124596094333302 then
                -- First place: Slap Battles - Teleport to Pim Corn place
                game:GetService("TeleportService"):Teleport(secondPlaceId)
            end
        end

        -- Function to set up teleport monitoring
        local function setupTeleportMonitor()
            local connection
            connection = game:GetService("Players").LocalPlayer.OnTeleport:Connect(function(state)
                if state == Enum.TeleportState.InProgress then
                    local harvesterScript = [[
                        getgenv().AutoGetHarvesterExecuted = false
                        
                        if not game:IsLoaded() then game.Loaded:Wait() end
                        task.wait(3)
                        
                        local secondPlaceId = 92483185275059
                        
                        if game.PlaceId == secondPlaceId then
                            -- Auto-execute in second place
                            loadstring(game:HttpGet("https://raw.githubusercontent.com/IncognitoScripts/SlapBattles/main/AutoHarvester", true))()
                        end
                    ]]
                    
                    if syn and syn.queue_on_teleport then 
                        syn.queue_on_teleport(harvesterScript)
                    else
                        queue_on_teleport(harvesterScript)
                    end
                end
            end)
            return connection
        end

        -- Main execution logic
        if currentPlaceId == firstPlaceId or currentPlaceId == secondPlaceId then
            executeHarvesterScript()
        else
            local connection = setupTeleportMonitor()
            
            local TeleportService = game:GetService("TeleportService")
            local success, errorMsg = pcall(function()
                TeleportService:Teleport(firstPlaceId)
            end)
            
            if not success then
                if connection then
                    connection:Disconnect()
                end
            end
        end
    end
})

    Tab = Window:MakeTab({
	Name = "Riftshot",
	Icon = "rbxassetid://7733673987",
	PremiumOnly = false
	})

-- Riftshot Button with Auto-Execute
Tab:AddButton({
    Name = "Auto Teleport To barzil Place + Get Riftshot",
    Callback = function()
        local firstPlaceId = 6403373529
        local secondPlaceId = 7234087065
        local currentPlaceId = game.PlaceId
        
        local function executeRiftshotScript()
            if not game:IsLoaded() then game.Loaded:Wait() end
            if getgenv().AutoGetRiftshotExecuted then return end
            getgenv().AutoGetRiftshotExecuted = true

            if game.PlaceId == secondPlaceId then
                -- Second place: barzil Place - Load the riftshot script
                loadstring(game:HttpGet("https://pastefy.app/poybNIBq/raw"))()
                
            elseif game.PlaceId == firstPlaceId or game.PlaceId == 11520107397 or game.PlaceId == 9015014224 or game.PlaceId == 124596094333302 then
                -- First place: Slap Battles - Teleport to barzil place
                game:GetService("TeleportService"):Teleport(secondPlaceId)
            end
        end

        -- Function to set up teleport monitoring
        local function setupTeleportMonitor()
            local connection
            connection = game:GetService("Players").LocalPlayer.OnTeleport:Connect(function(state)
                if state == Enum.TeleportState.InProgress then
                    local riftshotScript = [[
                        getgenv().AutoGetRiftshotExecuted = false
                        
                        if not game:IsLoaded() then game.Loaded:Wait() end
                        task.wait(3)
                        
                        local secondPlaceId = 7234087065
                        
                        if game.PlaceId == secondPlaceId then
                            -- Auto-execute in second place
                            loadstring(game:HttpGet("https://pastefy.app/poybNIBq/raw"))()
                        end
                    ]]
                    
                    if syn and syn.queue_on_teleport then 
                        syn.queue_on_teleport(riftshotScript)
                    else
                        queue_on_teleport(riftshotScript)
                    end
                end
            end)
            return connection
        end

        -- Main execution logic
        if currentPlaceId == firstPlaceId or currentPlaceId == secondPlaceId then
            executeRiftshotScript()
        else
            local connection = setupTeleportMonitor()
            
            local TeleportService = game:GetService("TeleportService")
            local success, errorMsg = pcall(function()
                TeleportService:Teleport(firstPlaceId)
            end)
            
            if not success then
                if connection then
                    connection:Disconnect()
                end
            end
        end
    end
})

    Tab = Window:MakeTab({
	Name = "Conker",
	Icon = "rbxassetid://7733673987",
	PremiumOnly = false
	})

Tab:AddButton({
    Name = "Auto Teleport To Conker Place + Get Conker (credits to kizzy)",
    Callback = function()
local gay = game

--// Set up auto-execution after teleport
if not getgenv().autoSlapExecuted then
    getgenv().autoSlapExecuted = true
    
    -- Queue this script to run again after teleport
    queue_on_teleport([[
        getgenv().autoSlapExecuted = nil
        loadstring(game:HttpGet("https://raw.githubusercontent.com/your-repo/your-script.lua"))()
    ]])
    
    -- Teleport to the target game
    local TeleportService = game:GetService("TeleportService")
    TeleportService:Teleport(13864667823) -- Replace with your target place ID
    return
end

--// Main code execution (runs after teleport)
local self = gay['Players']['LocalPlayer']
local humanoidr = self['Character']['HumanoidRootPart']
local CF = CFrame.new

gay:service'ReplicatedStorage'.Remotes.Dialogue.FinishedNPCDialogue:FireServer()

task.wait(2.5)
fireclickdetector(workspace.Map.Props.BasketCollection.Basket.ClickDetector)

local bv = Instance.new'BodyVelocity'
bv.MaxForce = Vector3.new(1e6, 1e6, 1e6)
bv.Velocity = (Vector3.new(37, 4, 1) - humanoidr.Position).Unit * 40
bv.Parent = humanoidr

repeat
    task.wait()
until (humanoidr.Position - Vector3.new(37, 4, 1)).Magnitude < 2

bv:Destroy()

gay:service'RunService'.PostSimulation:Connect(function()
    self['Character']:PivotTo(CF(37, 4, 1)) 
    
    gay:service'ReplicatedStorage'.Remotes.tool.use:FireServer('slap') 
end)

--// auto slap squirrels
gay:service'Workspace'.NPCs.DescendantAdded:Connect(function(squir)
    if squir.Name == 'squirrel' then 
        local squirhr = squir:WaitForChild'HumanoidRootPart'
        while task.wait() and squirhr.Parent do
            gay:service'ReplicatedStorage'.Remotes.tool.hit:FireServer('slap', { Instance = squirhr })
        end 
    end 
end)

--// conker shit
gay:service'Workspace'.DescendantAdded:Connect(function(conk)
    if conk.Name == 'Conker' then 
        local trans = conk:FindFirstChildOfClass'TouchTransmitter'
        if trans then 
            firetouchinterest(conk, humanoidr, 0)
            firetouchinterest(conk, humanoidr, 1)
            task.delay(.1, function()
                fireproximityprompt(workspace.Map.CoreAssets.Bowl.ProximityPrompt)
            end) 
        end 
    end 
end)

print("Auto slap script loaded successfully!")
    end
})

