local Tab = Window:MakeTab({
    Name = "Moon",
    Icon = "rbxassetid://7734068321",
    PremiumOnly = false
})

Section = Tab:AddSection({
    Name = "1-2 Quest"
})

Tab:AddToggle({
    Name = "Auto Ragdoll + Touch Ground",
    Default = false,
    Callback = function(Value)
        if Value then
            -- Start the loop
            autoRagdollLoop = game:GetService("RunService").Heartbeat:Connect(function()
                -- Check if player has entered
                if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("entered") then
                    -- Fire knockback remote
                    game.ReplicatedStorage.SelfKnockback:FireServer({
                        ["Force"] = 0,
                        ["Direction"] = Vector3.new(0, 0.01, 0)
                    })
                    
                    -- Teleport to position
                    if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1, -5, 8)
                    end
                end
            end)
        else
            -- Stop the loop
            if autoRagdollLoop then
                autoRagdollLoop:Disconnect()
                autoRagdollLoop = nil
            end
        end
    end
})

Tab:AddToggle({
    Name = "Auto Spam Equip/Unequip Moon Glove",
    Default = false,
    Callback = function(Value)
        if Value then
            -- Start the loop
            moonGloveLoop = game:GetService("RunService").Heartbeat:Connect(function()
                local player = game.Players.LocalPlayer
                local backpack = player:FindFirstChild("Backpack")
                
                if backpack then
                    local moonTool = backpack:FindFirstChild("Moon")
                    
                    if moonTool then
                        -- Check if tool is currently equipped
                        local character = player.Character
                        local currentlyEquipped = character and character:FindFirstChild("Moon")
                        
                        if currentlyEquipped then
                            -- Unequip: move tool back to backpack
                            moonTool.Parent = backpack
                        else
                            -- Equip: move tool to character
                            moonTool.Parent = character
                        end
                    end
                end
            end)
        else
            -- Stop the loop
            if moonGloveLoop then
                moonGloveLoop:Disconnect()
                moonGloveLoop = nil
            end
        end
    end
})

