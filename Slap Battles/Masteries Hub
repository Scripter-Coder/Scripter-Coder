Section = Tab:AddSection({ Name = "1/2 Quest" })

Tab:AddToggle({
    Name = "Auto Slap Players",
    Default = false,
    Callback = function(AutoSlapStalker)
        local running = AutoSlapStalker

        local function teleportToRandomPlayer()
            local Players = game:GetService("Players")
            local playerList = Players:GetPlayers()
            if #playerList > 1 then
                local randomPlayer
                repeat
                    randomPlayer = playerList[math.random(1, #playerList)]
                until randomPlayer ~= Players.LocalPlayer

                local randomPlayerCharacter = randomPlayer.Character
                if randomPlayerCharacter and randomPlayerCharacter:FindFirstChild("HumanoidRootPart") then
                    local teleportPosition = randomPlayerCharacter.HumanoidRootPart.Position
                    Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(teleportPosition)
                end
            end
        end

        local function fireGeneralHitForAllPlayers()
            local Players = game:GetService("Players")
            local ReplicatedStorage = game:GetService("ReplicatedStorage")
            for _, player in pairs(Players:GetPlayers()) do
                if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                    ReplicatedStorage:WaitForChild("GeneralHit"):FireServer(player.Character.HumanoidRootPart)
                end
            end
        end

        local function runAutoSlapStalkerLoop()
            if game.Players.LocalPlayer.leaderstats.Glove.Value == "Stalker" then
                while running do
                    wait(0.2)
                    teleportToRandomPlayer()
                    fireGeneralHitForAllPlayers()
                    wait(1)
                end
            else
                OrionLib:MakeNotification({ Name = "Error", Content = "Equip Stalker Glove", Image = "rbxassetid://7743878857", Time = 3 })
            end
        end

        if AutoSlapStalker then
            running = true
            runAutoSlapStalkerLoop()
        else
            running = false
        end
    end
})

local Tab = Window:MakeTab({
    Name = "Cherry",
    Icon = "rbxassetid://7734068321",
    PremiumOnly = false
})

Section = Tab:AddSection({
    Name = "1-2-3 Quest"
})

local running = false -- declare global toggle state

Tab:AddToggle({
    Name = "Auto Spawn Cherry Clones",
    Default = false,
    Callback = function(Value)
        if Value then
            -- Start the loop with exact 0.1 second interval
            cherryCloneLoop = true
            
            spawn(function()
                while cherryCloneLoop do
                    local player = game.Players.LocalPlayer
                    
                    -- Check if player has Cherry glove
                    if player.leaderstats and player.leaderstats.Glove and player.leaderstats.Glove.Value == "Cherry" then
                        -- Check if player has entered
                        if player.Character and player.Character:FindFirstChild("entered") then
                            -- Fire the Cherry remote
                            game:GetService("ReplicatedStorage").Events.Cherry:FireServer()
                        else
                            OrionLib:MakeNotification({
                                Name = "Not Entered",
                                Content = "You need to enter the arena first!",
                                Image = "rbxassetid://4483345998",
                                Time = 3
                            })
                            wait(1) -- Wait longer when not entered
                        end
                    else
                        OrionLib:MakeNotification({
                            Name = "Wrong Glove",
                            Content = "You need Cherry glove equipped!",
                            Image = "rbxassetid://4483345998",
                            Time = 3
                        })
                        wait(1) -- Wait longer when wrong glove
                    end
                    
                    wait(0.1) -- Normal 0.1 second delay
                end
            end)
        else
            -- Stop the loop
            cherryCloneLoop = false
        end
    end
})

local Tab = Window:MakeTab({
    Name = "Booster",
    Icon = "rbxassetid://7734068321",
    PremiumOnly = false
})

Section = Tab:AddSection({
    Name = "1 Quest"
})

local running = false -- declare global toggle state

Tab:AddToggle({
    Name = "Ambush Players 750 Times",
    Default = false,
    Callback = function(AutoSlapBooster)
        running = AutoSlapBooster

        if not running then
            return -- stop immediately if toggled off
        end

        -- Run everything in a separate thread
        task.spawn(function()
            local Players = game:GetService("Players")
            local ReplicatedStorage = game:GetService("ReplicatedStorage")
            local LocalPlayer = Players.LocalPlayer

            -- Function to teleport to a random player
            local function teleportToRandomPlayer()
                local playerList = Players:GetPlayers()
                if #playerList > 1 then
                    local randomPlayer = nil
                    repeat
                        randomPlayer = playerList[math.random(1, #playerList)]
                    until randomPlayer ~= LocalPlayer

                    local targetChar = randomPlayer.Character
                    if targetChar and targetChar:FindFirstChild("HumanoidRootPart") then
                        LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(
                            targetChar.HumanoidRootPart.Position + Vector3.new(0, 3, 0)
                        )
                    end
                end
            end

            -- Function to fire GeneralHit for all players
            local function fireGeneralHitForAllPlayers()
                for _, player in pairs(Players:GetPlayers()) do
                    if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                        ReplicatedStorage:WaitForChild("GeneralHit"):FireServer(player.Character.HumanoidRootPart)
                    end
                end
            end

            -- Loop 750 times or until stopped
            local count = 0
            while running and count < 750 do
                if LocalPlayer.leaderstats.Glove.Value == "Booster" then
                    teleportToRandomPlayer()
                    fireGeneralHitForAllPlayers()
                    count += 1
                    task.wait(2.1)
                else
                    OrionLib:MakeNotification({
                        Name = "Error",
                        Content = "Equip Booster Glove",
                        Image = "rbxassetid://7743878857",
                        Time = 3
                    })
                    break
                end
            end
        end)
    end
})

