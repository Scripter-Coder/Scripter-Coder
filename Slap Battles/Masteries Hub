local AutoAbiltySlapHiveRunning = false
Tab:AddToggle({
    Name = "Auto Ability + Slap People",
    Default = false,
    Callback = function(Value)
        AutoAbiltySlapHiveRunning = Value
        local player = game.Players.LocalPlayer

        if AutoAbiltySlapHiveRunning then
            if player.leaderstats.Glove.Value == "Hive" then
                task.spawn(function()
                    while AutoAbiltySlapHiveRunning do
                        local players = game:GetService("Players"):GetPlayers()
                        local targetPlayer
                        repeat
                            targetPlayer = players[math.random(1, #players)]
                        until targetPlayer ~= player and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart")

                        local targetHRP = targetPlayer.Character.HumanoidRootPart
                        player.Character.HumanoidRootPart.CFrame = CFrame.new(targetHRP.Position)

                        task.wait(1)
                    end

                    while true do
                        game:GetService("ReplicatedStorage").GeneralAbility:FireServer()
                        task.wait(1)
                        game:GetService("ReplicatedStorage").GeneralAbility:FireServer()
                        task.wait(14)
                    end
                end)
            else
                AutoAbiltySlapHiveRunning = false
                OrionLib:MakeNotification({ Name = "Error", Content = "Equip Hive Glove", Image = "rbxassetid://7743878857", Time = 3 })
            end
        end
    end
})

