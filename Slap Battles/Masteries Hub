    PremiumOnly = false
})

Section = Tab:AddSection({
    Name = "1/2/3 Quest"
})

Tab:AddButton({
    Name = "Throw Bomb/Explode Bomb",
    Callback = function()
        if game.Players.LocalPlayer.leaderstats.Glove.Value == "Bomb" then
            game:GetService("ReplicatedStorage").BombThrow:FireServer("Ebutton")
        else
            OrionLib:MakeNotification({
                Name = "Error",
                Content = "Equip Bomb",
                Image = "rbxassetid://7743878857",
                Time = 3
            })
        end
    end
})

Tab:AddButton({
    Name = "Auto Do 1/2/3 Tasks",
    Callback = function(AutoBomb)
        if game.Players.LocalPlayer.leaderstats.Glove.Value == "Bomb" then
            while AutoBomb do
                local player = game.Players.LocalPlayer

                -- Move to safe area
                player.Character.HumanoidRootPart.CFrame = CFrame.new(-908, 328, 5)
                wait(1)

                -- Move to platform
                player.Character.HumanoidRootPart.CFrame = CFrame.new(10019, -42, 9960)
                wait(1)

                -- Throw Bomb
                game:GetService("ReplicatedStorage").BombThrow:FireServer("Ebutton")
                wait(1)

                -- Wait to teleport to bomb
                while true do
                    local foundBomb = false

                    for _, v in pairs(game.Workspace:GetChildren()) do
                        if string.find(v.Name, "_bÃ¸mb") then
                            player.Character.HumanoidRootPart.CFrame = v.CFrame
                            foundBomb = true
                            break
                        end
                    end

                    if not foundBomb then
                        wait(1)
                    else
                        wait()
                    end
                end

                wait(9)
                player.Character.Humanoid.Health = 0
                wait(23)
            end
        else
            OrionLib:MakeNotification({
                Name = "Error",
                Content = "Equip Bomb Glove",
                Image = "rbxassetid://7743878857",
                Time = 3
            })
        end
    end
})

