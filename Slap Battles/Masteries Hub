Section = Tab:AddSection({ Name = "2-3 Quest" })

local AutoWall = false
Tab:AddToggle({
    Name = "Auto Build And Destroy Wall",
    Default = false,
    Callback = function(Value)
        AutoWall = Value
        local player = game.Players.LocalPlayer

        if AutoWall then
            if player.leaderstats.Glove.Value == "Fort" then
                task.spawn(function()
                    while AutoWall do
                        player.Character.HumanoidRootPart.CFrame = CFrame.new(10019, -42, 9960)
                        wait(1)
                        game:GetService("ReplicatedStorage").Fortlol:FireServer()
                        wait(1)
                        player.Character.HumanoidRootPart.CFrame = CFrame.new(10018, -42, 9951)
                        wait(1)
                        game.ReplicatedStorage.SelfKnockback:FireServer({ Force = 0, Direction = Vector3.new(0, 0.01, 0) })
                        wait(4)

                        if player.leaderstats.Glove.Value ~= "Fort" then
                            AutoWall = false
                            OrionLib:MakeNotification({ Name = "Error", Content = "Equip Fort Glove", Image = "rbxassetid://7743878857", Time = 3 })
                        end
                    end
                end)
            else
                OrionLib:MakeNotification({ Name = "Error", Content = "Equip Fort Glove", Image = "rbxassetid://7743878857", Time = 3 })
            end
        end
    end
})

