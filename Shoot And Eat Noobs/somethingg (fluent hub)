    Title = "Max Distance",
    Default = "1000",
    Placeholder = "Max distance (10-5000)",
    Numeric = true,
    Finished = false,
    Callback = function(Text)
        local distance = tonumber(Text) or 1000
        AnniversarySettings.MaxDistance = math.clamp(distance, 10, 5000)
    end
