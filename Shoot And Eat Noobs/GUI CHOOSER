-- SAEN Script Loader GUI
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")
local OrionButton = Instance.new("TextButton")
local FluentButton = Instance.new("TextButton")

-- Create ScreenGui
ScreenGui.Parent = game.CoreGui
ScreenGui.Name = "SAENLoader"
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

-- Create Main Frame
Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
Frame.BorderColor3 = Color3.fromRGB(20, 20, 20)
Frame.Position = UDim2.new(0.35, 0, 0.35, 0)
Frame.Size = UDim2.new(0, 300, 0, 200)
Frame.Active = true
Frame.Draggable = true

-- Create Title Text
TextLabel.Parent = Frame
TextLabel.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
TextLabel.BorderColor3 = Color3.fromRGB(30, 30, 30)
TextLabel.Size = UDim2.new(1, 0, 0, 50)
TextLabel.Font = Enum.Font.GothamBold
TextLabel.Text = "Hello User How Would U Like To Run SAEN Script?"
TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.TextSize = 14
TextLabel.TextWrapped = true

-- Create Orion Hub Button
OrionButton.Parent = Frame
OrionButton.BackgroundColor3 = Color3.fromRGB(60, 60, 200)
OrionButton.BorderColor3 = Color3.fromRGB(40, 40, 150)
OrionButton.Position = UDim2.new(0.1, 0, 0.4, 0)
OrionButton.Size = UDim2.new(0.8, 0, 0, 35)
OrionButton.Font = Enum.Font.Gotham
OrionButton.Text = "1. Orion Hub"
OrionButton.TextColor3 = Color3.fromRGB(255, 255, 255)
OrionButton.TextSize = 14

-- Create Fluent Hub Button
FluentButton.Parent = Frame
FluentButton.BackgroundColor3 = Color3.fromRGB(200, 60, 60)
FluentButton.BorderColor3 = Color3.fromRGB(150, 40, 40)
FluentButton.Position = UDim2.new(0.1, 0, 0.7, 0)
FluentButton.Size = UDim2.new(0.8, 0, 0, 35)
FluentButton.Font = Enum.Font.Gotham
FluentButton.Text = "2. Fluent Hub"
FluentButton.TextColor3 = Color3.fromRGB(255, 255, 255)
FluentButton.TextSize = 14

-- Close button (optional)
local CloseButton = Instance.new("TextButton")
CloseButton.Parent = Frame
CloseButton.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
CloseButton.BorderColor3 = Color3.fromRGB(70, 70, 70)
CloseButton.Position = UDim2.new(0.9, -20, 0.02, 0)
CloseButton.Size = UDim2.new(0, 20, 0, 20)
CloseButton.Font = Enum.Font.Gotham
CloseButton.Text = "X"
CloseButton.TextColor3 = Color3.fromRGB(255, 255, 255)
CloseButton.TextSize = 12

-- Button functions
local function loadOrionHub()
    -- Replace this with your actual Orion Hub loadstring
    local orionLoadstring = "loadstring(game:HttpGet('https://raw.githubusercontent.com/Scripter-Coder/Scripter-Coder/refs/heads/main/Shoot%20And%20Eat%20Noobs/Shoot%20And%20Eat%20Noobs%20Hub%20Script'))()"
    
    -- Execute the loadstring
    local success, err = pcall(function()
        loadstring(orionLoadstring)()
    end)
    
    if success then
        TextLabel.Text = "Orion Hub loaded successfully!"
        wait(2)
        ScreenGui:Destroy()
    else
        TextLabel.Text = "Error loading Orion Hub!"
        warn("Orion Hub Error: " .. err)
    end
end

local function loadFluentHub()
    -- Replace this with your actual Fluent Hub loadstring
    local fluentLoadstring = "loadstring(game:HttpGet('https://raw.githubusercontent.com/Scripter-Coder/Scripter-Coder/refs/heads/main/Shoot%20And%20Eat%20Noobs/Shoot%20And%20Eat%20Noobs%20(Fluent%20Version)'))()"
    
    -- Execute the loadstring
    local success, err = pcall(function()
        loadstring(fluentLoadstring)()
    end)
    
    if success then
        TextLabel.Text = "Fluent Hub loaded successfully!"
        wait(2)
        ScreenGui:Destroy()
    else
        TextLabel.Text = "Error loading Fluent Hub!"
        warn("Fluent Hub Error: " .. err)
    end
end

local function closeGUI()
    ScreenGui:Destroy()
end

-- Connect button events
OrionButton.MouseButton1Click:Connect(loadOrionHub)
FluentButton.MouseButton1Click:Connect(loadFluentHub)
CloseButton.MouseButton1Click:Connect(closeGUI)

-- Button hover effects
OrionButton.MouseEnter:Connect(function()
    OrionButton.BackgroundColor3 = Color3.fromRGB(80, 80, 220)
end)

OrionButton.MouseLeave:Connect(function()
    OrionButton.BackgroundColor3 = Color3.fromRGB(60, 60, 200)
end)

FluentButton.MouseEnter:Connect(function()
    FluentButton.BackgroundColor3 = Color3.fromRGB(220, 80, 80)
end)

FluentButton.MouseLeave:Connect(function()
    FluentButton.BackgroundColor3 = Color3.fromRGB(200, 60, 60)
end)

CloseButton.MouseEnter:Connect(function()
    CloseButton.BackgroundColor3 = Color3.fromRGB(255, 80, 80)
end)

CloseButton.MouseLeave:Connect(function()
    CloseButton.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
end)

print("SAEN Script Loader GUI loaded! Drag the window to move it.")
