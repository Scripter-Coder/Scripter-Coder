--AUTO FARM
local Tab = Window:MakeTab({
	Name = "Auto Farm/Get",
	Icon = "rbxassetid://7733946818",
	PremiumOnly = false
	})

local Section = Tab:AddSection({
	Name = "Auto Get/Farm Gloves/Badges"
})

Tab:AddToggle({
    Name = "Auto Get Megarock Glove",
    Default = false,
    Callback = function(Value)
if game.Players.LocalPlayer.leaderstats.Glove.Value == "Diamond" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(10001, -42, 10000)
game:GetService("ReplicatedStorage").Rockmode:FireServer()
else
            OrionLib:MakeNotification({
                Name = "Tip",
                Content = "Equip Diamond",
                Image = "rbxassetid://7733975185",
                Time = 3
            })
        end
    end
})

Tab:AddToggle({
	Name = "Auto Get Mitten/Snowball Gloves",
	Default = false,
	Callback = function(Value)
Giftfarm = Value
while Giftfarm do
for i,v in pairs(game.Workspace:GetChildren()) do
                    if v.Name == "Gift" then
firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"), v, 0)
firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"), v, 1)
                    end
                end
task.wait()
end
end
})

Tab:AddToggle({
	Name = "Auto Get Phase/Jet Gloves",
	Default = false,
	Callback = function(Value)
_G.PhaseOrJetfarm = Value
while _G.PhaseOrJetfarm do
for i,v in pairs(game.Workspace:GetChildren()) do
                    if v.Name == "JetOrb" or v.Name == "PhaseOrb" then
firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("Head"), v, 0)
firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("Head"), v, 1)
                    end
                end
task.wait()
end
	end    
})

Tab:AddToggle({
    Name = "Auto Get Bubble Glove",
    Default = false,
    Callback = function(Value)
if game.Players.LocalPlayer.leaderstats.Glove.Value == "Killstreak" then
wait(0.2)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(10001, -42, 10000)
wait(0.5)
        _G.AutoFarmKill = Value
        
        if _G.AutoFarmKill then
            if _G.ClosestMagnitude == nil then
                _G.ClosestMagnitude = 99999
            end
        else
            if _G.ClosestMagnitude then
                _G.ClosestMagnitude = nil
            end
        end
        
        while _G.AutoFarmKill do
            for _, player in pairs(game.Players:GetPlayers()) do
                if player ~= game.Players.LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") and player.Character:FindFirstChild("entered") then
                    local magnitude = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - player.Character.HumanoidRootPart.Position).Magnitude
                    
                    if magnitude <= _G.ClosestMagnitude then
                        _G.ClosestMagnitude = magnitude
                        RandomPlayer = player
                    end
                end
            end
            
            if RandomPlayer and RandomPlayer.Character then
                local randomChar = RandomPlayer.Character
                local randomHead = randomChar:FindFirstChild("Head")
                local randomHumanoid = randomChar:FindFirstChild("Humanoid")
                
                if randomHead and randomHumanoid and randomHumanoid.Health > 0 and randomChar:FindFirstChild("entered") then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = randomHead.CFrame
                    wait(0.19)
                    game.ReplicatedStorage.KSHit:FireServer(randomHead)
                    
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace["Reack"].CFrame * CFrame.new(0, 5, 0)
                end
            else
                _G.ClosestMagnitude = 999999
                RandomPlayer = nil
            end
            
            task.wait(0.4)
        end
else
        if not _G.AutoFarmKill then
            OrionLib:MakeNotification({
                Name = "Tip",
                Content = "Equip Killstreak",
                Image = "rbxassetid://7733975185",
                Time = 3
            })
        end
    end
end
})

Tab:AddButton({
    Name = "Auto Get Bubble Glove 2 (Slap Aura)",
    Callback = function()
if game.Players.LocalPlayer.leaderstats.Glove.Value == "Killstreak" then
-- Get the ReplicatedStorage and Players service
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Players = game:GetService("Players")

-- Start the infinite loop to fire for all players
while true do
    -- Loop through each player in the game
    for _, player in pairs(Players:GetPlayers()) do
        -- Ensure the player has a character and it contains the "Right Arm"
        if player.Character and player.Character:FindFirstChild("Right Arm") then
            -- Create the arguments table for the FireServer function
            local args = {
                [1] = player.Character["Right Arm"]
            }

            -- Fire the server with the "Right Arm" part as an argument
            ReplicatedStorage.KSHit:FireServer(unpack(args))
        end
    end

    -- Wait 1 second before running the loop again (adjust the wait time as needed)
    wait(1)
end
else
OrionLib:MakeNotification({Name = "Tip",Content = "Equip Killstreak",Image = "rbxassetid://7733975185",Time = 3})
end
end
})


Tab:AddToggle({
	Name = "Auto Get Potato Glove",
	Default = false,
	Callback = function(Value)   
if game.Players.LocalPlayer.leaderstats.Glove.Value == "Reverse" then
Giftfarm = Value
while Giftfarm do
for i,v in pairs(game.Workspace:GetChildren()) do
                    if v.Name == "BubbleObject" then
firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"), v, 0)
firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"), v, 1)
                    end
                end
task.wait()
end
else
