end
})

Tab:AddButton({
    Name = "Get Glove Trap (1)",
    Callback = function()
        wait(0.3)
      if game.Players.LocalPlayer.leaderstats.Slaps.Value >= 60000 then
        fireclickdetector(workspace.Lobby["God's Hand"].ClickDetector) -- god hand
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-909, 328, 3) -- tp to arena portal
        task.wait(0.5) -- 0.5 seconds
        game:GetService("ReplicatedStorage").TimestopJump:FireServer() -- time stop (jump)
        game:GetService("ReplicatedStorage").Timestopchoir:FireServer() -- time stop (unwalk)
        game:GetService("ReplicatedStorage").Timestop:FireServer() -- time stop (fully)
        game.Players.LocalPlayer.Character.Humanoid.Health = 0 -- reset player
        task.wait(6) -- 6 seconds
        fireclickdetector(workspace.Lobby["Brick"].ClickDetector) -- brick
        task.wait(0.1) -- 0.1 seconds
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-909, 328, 3) -- tp to arena portal
            
            -- Fixed 'repeat' block for waiting until 'entered' is found
            repeat
                wait(0.5) -- 0.5 seconds
            until game.Players.LocalPlayer.Character:FindFirstChild("entered") -- entered arena

            wait(2) -- 2 seconds

            for i = 1, 10000 do -- 10000 clones spawn
            game:GetService("ReplicatedStorage").lbrick:FireServer()
            wait(0.1)  -- Adjust this wait time to make the loop slower or faster
        end

        -- Optional: Rejoin script (only if necessary)
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Jelly-plays/Rejoin-script/main/obf_L744By559M18BbuseSG6en8r1zL31daK9060LV7WyvmS4bQp92aONWfwRE36FdcZ.lua.txt"))()

        -- Show a notification
        OrionLib:MakeNotification({
            Name = "Tip",
            Content = "Equip Brick/God Hand",
            Image = "rbxassetid://7733975185",
            Time = 3
        })
    end
    end
})

