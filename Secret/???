--INSTANT
local OrionLib = loadstring(game:HttpGet("https://gist.githubusercontent.com/Scripter-Coder/39eb43cc6530717e76d79899a794d4e1/raw/222482765b9cb8e249bba197240a5b712b4722e7/Orion%2520Library%2520Source%2520From%2520Start"))()

if game.PlaceId == 6403373529 or game.PlaceId == 9015014224 or game.PlaceId == 11520107397 then --Slap Battles
local Window = OrionLib:MakeWindow({Name = "Badges Hub V. 5.73 W.I.P ", HidePremium = true, IntroEnabled = true, IntroText = "Slap Battles ", IntroIcon = "rbxassetid://15315284749", ToggleIcon = "assetid://7734091286"})

local Tab = Window:MakeTab({
	Name = "Instant",
	Icon = "rbxassetid://7733946818",
	PremiumOnly = false
	})

local Section = Tab:AddSection({
	Name = "Instant Gloves/Badges"
})

OrionLib:MakeNotification({
	Name = "Scripter",
	Content = "Good luck :)",
	Image = "rbxassetid://4483345998",
	Time = 3
})

Tab:AddButton({
    Name = "Get Acrobat Glove",
    Callback = function()   
        if game.Players.LocalPlayer.InGroup.Value == true then
            fireclickdetector(workspace.Lobby["Acrobat"].ClickDetector)
        else
            OrionLib:MakeNotification({
                Name = "Tip",
                Content = "Join Group/Like Game/Fav Game/Follow Tencell",
                Image = "rbxassetid://7733975185",
                Time = 3
            })
        end
    end
})

Tab:AddButton({
	Name = "Get [Redacted] Glove",
	Callback = function()
if game.Players.LocalPlayer.leaderstats.Slaps.Value >= 5000 then
Door = 0
for i = 1, 10 do
Door = Door + 1
if game:GetService("BadgeService"):UserHasBadgeAsync(game.Players.LocalPlayer.UserId, 2124847850) then
Door = nil
else
firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("Head"), workspace.PocketDimension.Doors[Door].TouchInterest.Parent, 0)
firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("Head"), workspace.PocketDimension.Doors[Door].TouchInterest.Parent, 1)
wait(3.75)
end
end
else
OrionLib:MakeNotification({Name = "Tip",Content = "5000 Slaps Required",Image = "rbxassetid://7733975185",Time = 3})
end
  	end    
})

Tab:AddButton({
    Name = "Get Glove Bob (1)",
    Callback = function()
        -- waiting to load game
        if not game:IsLoaded() then
            game.Loaded:Wait()
        end
        print("game loaded")

        if game.Players.LocalPlayer.leaderstats.Slaps.Value >= 60000 then
            fireclickdetector(workspace.Lobby["God's Hand"].ClickDetector) -- god hand
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-909, 328, 3) -- tp to arena portal
            task.wait(0.5) -- 0.5 seconds
            game:GetService("ReplicatedStorage").TimestopJump:FireServer() -- time stop (jump)
            game:GetService("ReplicatedStorage").Timestopchoir:FireServer() -- time stop (unwalk)
            game:GetService("ReplicatedStorage").Timestop:FireServer() -- time stop (fully)
            game.Players.LocalPlayer.Character.Humanoid.Health = 0 -- reset player
            task.wait(6) -- 6 seconds
            fireclickdetector(workspace.Lobby["Replica"].ClickDetector) -- replica
            task.wait(0.1) -- 0.1 seconds
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-909, 328, 3) -- tp to arena portal
            
            -- Fixed 'repeat' block for waiting until 'entered' is found
            repeat
                wait(0.5) -- 0.5 seconds
            until game.Players.LocalPlayer.Character:FindFirstChild("entered") -- entered arena

            wait(2) -- 2 seconds
            for i = 1, 7500 do -- 7500 clones spawn
                game:GetService("ReplicatedStorage").Duplicate:FireServer(unpack({[1] = true})) -- spawn clone
            end
            wait(3.5) -- 3.5 seconds
loadstring(game:HttpGet("https://raw.githubusercontent.com/Jelly-plays/Rejoin-script/main/obf_L744By559M18BbuseSG6en8r1zL31daK9060LV7WyvmS4bQp92aONWfwRE36FdcZ.lua.txt"))();
            OrionLib:MakeNotification({
                Name = "Tip",
                Content = "Equip Replica/God Hand",
                Image = "rbxassetid://7733975185",
                Time = 3
            })
        end
    end
}) -- Don't forget this 'end' to close the callback function

